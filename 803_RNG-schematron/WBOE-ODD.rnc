     | tei_model.global)*,
    tei_att.global.attributes,
    tei_att.lexicographic.attributes,
    
    ## classifies form as simple, compound, etc.
    attribute type {
      
      ## word in other than usual dictionary form
      "inflected"
      | 
        ## the headword of the article
        "lemma"
      | 
        ## a variant form of the headword, can have subtypes
        "variant"
      | 
        ## instance of a dialect form for a given location
        "dialect"
    },
    
    ## provides a sub-categorization of the element, if needed
    attribute subtype {
      
      ##
      "diminutive"
      | 
        ##
        "kurzwort"
      | 
        ##
        "diminuiertes kurzwort"
      | 
        ##
        "diminuierte kurzform"
      | 
        ##
        "nebenform"
      | 
        ## Movierung is a derivation to define the specific gender of a person. e.g. mister -> mistress
        "movierung"
      | 
        ##
        "inflected"
      | 
        ##
        "compound"
      | 
        ##
        "MWE"
    }?,
    empty
