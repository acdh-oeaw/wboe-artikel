  <text>
    <body>
      <entry>
        <form type="lemma" xml:id="">
          <orth></orth>
        </form>
        <usg type="geo">
          <placeName></placeName>
        </usg>
        <note type="kommentar"></note>
        <form type="dialect">
          <pron xml:lang="bar-AT"></pron>
        </form>
        <etym></etym>
        <sense>
          <sense></sense>
        </sense>
        <re>
          <form subtype="compound" type="lemma">
            <orth>gleichfassen</orth>
          </form>
          <sense>
            <sense>
              <def>dem Misthaufen mittels umschaufeln die gewünschte Form geben</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.3">WStmk.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.5">OStmk.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">n Mīsthaufn glaichfǫ̇ßn</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p3.3b12">Söding-St. Johann</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c23397"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>herfassen</orth>
          </form>
          <sense>
            <sense>
              <def>jemanden heftig packen und an sich ziehen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.1">Innv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c23398"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>heufassen</orth>
          </form>
          <sense>
            <sense>
              <def>eine Heuladung auf den Wagen hochreichen bzw. entgegennehmen, anordnen und schlichten</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1A.4">öSTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.1">ObKtn.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.3">WStmk.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.3">Waldv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p7">Bgl.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c23399"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>mistfassen</orth>
          </form>
          <sense>
            <sense>
              <def>eine Mistladung auf den Wagen hochreichen bzw. entgegennehmen, anordnen und schlichten</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1B.0">OTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.3">WStmk.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.5">OStmk.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.4">Mühlv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p6">NÖ</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c23400"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>umfassen</orth>
          </form>
          <sense>
            <sense>
              <sense>
                <def>etwas wenden, an eine andere Stelle schaufeln</def>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p3.1">sbair.ObStmk.</placeName>
                </usg>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
                </usg>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p6.2">Industriev.</placeName>
                </usg>
                <cit type="example">
                  <quote xml:lang="bar-AT">Ön Mist umfossn</quote>
                  <usg type="geo">
                    <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.1i06">Waidhofen an der Ybbs</placeName>
                  </usg>
                </cit>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c24107"/>
              </sense>
              <sense>
                <def>etwas umladen bzw. in einen anderen Behälter füllen; umsacken</def>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p5.3">Traunv.</placeName>
                </usg>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p6.3">Waldv.</placeName>
                </usg>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p6.4">Weinv.</placeName>
                </usg>
                <cit type="example">
                  <quote xml:lang="bar-AT">s' Drǫad fåstn maⁿ um</quote>
                  <usg type="geo">
                    <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.3b02">Gmunden</placeName>
                  </usg>
                </cit>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c24108"/>
              </sense>
            </sense>
            <sense>
              <def>etwas eine andere Umrahmungen bzw. Ummantelung geben (z. B. von Schmuck)</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.2">Industriev.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c24109"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>verfassen</orth>
          </form>
          <sense>
            <sense>
              <sense>
                <sense>
                  <def>die letzte Heulage in der Mittellinie der Fuhre schlichten</def>
                  <usg type="geo">
                    <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
                  </usg>
                  <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c24112"/>
                </sense>
                <sense>
                  <def>eine Fuhre schlecht beladen</def>
                  <usg type="geo">
                    <placeName type="grossregion" ref="/geodata-wboe.xml#p3.1">sbair.ObStmk.</placeName>
                  </usg>
                  <usg type="geo">
                    <placeName type="grossregion" ref="/geodata-wboe.xml#p6.3">Waldv.</placeName>
                  </usg>
                  <cit type="example">
                    <quote xml:lang="bar-AT">hosdi fafoßth</quote>
                    <usg type="geo">
                      <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.3a15">Kautzen</placeName>
                    </usg>
                  </cit>
                  <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c23394"/>
                </sense>
              </sense>
              <sense>
                <def>bei der Kartoffelernte die Knollen versehentlich mit Erde zudecken</def>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p6.3">Waldv.</placeName>
                </usg>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c24114"/>
              </sense>
            </sense>
            <sense>
              <def>ein Schriftstück gedanklich ausarbeiten und niederschreiben</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.2">Hausrv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.3">Traunv.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">an Briaf verfåssn</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.2a39">Traun</placeName>
                </usg>
              </cit>
              <cit type="example">
                <quote xml:lang="bar-AT">a Bökąntəʳ ö da Šdåd håd maⁿs fəʳfåsst (dö schriftlö ɒiⁿgåb)</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.3b02">Gmunden</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c24113"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>zufassen</orth>
          </form>
          <sense>
            <sense>
              <sense>
                <def>eine Gelegenheit beim Schopf packen</def>
              </sense>
              <sense>
                <sense>
                  <def>die letzte Heulage in der Mittellinie der Fuhre schlichten</def>
                  <usg type="geo">
                    <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
                  </usg>
                  <cit type="example">
                    <quote xml:lang="bar-AT">s Fuᵒdr zuᵃ-fåßn</quote>
                    <usg type="geo">
                      <placeName type="kleinregion" ref="/geodata-wboe.xml#p2.3d">uGurkt.</placeName>
                    </usg>
                  </cit>
                  <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c24117"/>
                </sense>
                <sense>
                  <def>Erde zu einem Haufen aufschaufeln</def>
                  <usg type="geo">
                    <placeName type="grossregion" ref="/geodata-wboe.xml#p7.3">SBgl.</placeName>
                  </usg>
                  <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c23411"/>
                </sense>
              </sense>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>zusammenfassen</orth>
          </form>
          <sense>
            <sense>
              <sense>
                <sense>
                  <def>etwas zu einem größeren Ganzen vereinigen; zusammensammeln, zusammenpacken, zusammenschaufeln</def>
                  <usg type="geo">
                    <placeName type="grossregion" ref="/geodata-wboe.xml#p6.4">Weinv.</placeName>
                  </usg>
                  <cit type="example">
                    <quote xml:lang="bar-AT">sī muⁱß eᵃschd zó̄mfossn</quote>
                    <usg type="geo">
                      <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.4b03">Haugsdorf</placeName>
                    </usg>
                  </cit>
                  <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c24122"/>
                </sense>
                <sense>
                  <def>die letzte Heulage in der Mittellinie der Fuhre schlichten</def>
                  <usg type="geo">
                    <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
                  </usg>
                  <usg type="geo">
                    <placeName type="grossregion" ref="/geodata-wboe.xml#p3.1">sbair.ObStmk.</placeName>
                  </usg>
                  <usg type="geo">
                    <placeName type="grossregion" ref="/geodata-wboe.xml#p6.2">Industriev.</placeName>
                  </usg>
                  <cit type="example">
                    <quote xml:lang="bar-AT">'s Fuᵒdr zom-fåßn</quote>
                    <usg type="geo">
                      <placeName type="kleinregion" ref="/geodata-wboe.xml#p2.3c">Krappfd.</placeName>
                    </usg>
                  </cit>
                  <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c24120"/>
                </sense>
              </sense>
              <sense>
                <def>mehrere Personen festnehmen, gefangen nehmen</def>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p5.2">Hausrv.</placeName>
                </usg>
                <cit type="example">
                  <quote xml:lang="bar-AT">d' šdǫ̃ntá̄n hǭ̃md hãid me ɒ bǭr hǭdɒlumpm̥ dsǭ̃mgvaßt</quote>
                  <usg type="geo">
                    <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.2b39">Schlatt</placeName>
                  </usg>
                </cit>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c"/>
              </sense>
            </sense>
            <sense>
              <def>das Gesagte bzw. Geschriebene in eine gekürzte Form bringen; ein Resümee ziehen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c24121"/>
            </sense>
          </sense>
        </re>
      </entry>
