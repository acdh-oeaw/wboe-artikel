            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">wann do dö Fahrerei oamal aufhöret!</quote>
              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p4.5d05">St. Gilgen</placeName>
              </usg>
            </cit>
            <cit type="example">
              <quote xml:lang="bar-AT">wås is den des fiər a Fårərẹⁱ</quote>
              <usg type="geo">
                <placeName type="kleinregion" ref="/geodata-wboe.xml#p6.5r">18., Währing</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c437"/>
          </sense>
          <sense>
            <def>unnützes, vieles Fahren</def>
            <usg type="geo">
