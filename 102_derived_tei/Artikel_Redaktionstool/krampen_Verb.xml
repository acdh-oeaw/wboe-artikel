  <text>
    <body>
      <entry>
        <form type="lemma" xml:id="krampen">
          <orth>krampen</orth>
        </form>
        <form type="lemma" xml:id="kraempen">
          <orth>krämpen</orth>
          <ptr type="collection" subtype="lemma" target="/collections-wboe.xml#c32981"/>
        </form>
        <gramGrp>
          <gram type="pos">Verb</gram>
          <iType>schwach</iType>
        </gramGrp>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p3">Stmk.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p5.2">Hausrv.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p5.4">Mühlv.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p6.3">Waldv.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
        </usg>
        <note type="kommentar">folgt</note>
        <etym>
          <seg type="desc">Ableitung von</seg>
          <cit type="etymon">
            <xr type="lemma">
              <ref target="Krampen.xml">Krampen</ref>
            </xr>
          </cit>
        </etym>
        <sense>
          <sense>
            <sense>
              <def>auflockern von Erde</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p3">Stmk.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.2">Hausrv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c33892"/>
            </sense>
          </sense>
          <sense>
            <sense>
              <def>kratzen; krallen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.3">WStmk.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c33893"/>
            </sense>
            <sense>
              <def>mit den Hufen scharren</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.3">WStmk.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c33891"/>
            </sense>
            <sense>
              <def>schwerfällig umhergehen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.4">Mühlv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c33894"/>
            </sense>
            <sense>
              <def>piksen; stechen; stupfen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">hai tuət κhʀõm̃pm</quote>
                <ref type="bibl" target="#Herzmansky1969">Herzmansky 1969
                  <citedRange>321</citedRange>
                </ref>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p2.2i07">Villach</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c33890"/>
            </sense>
          </sense>
          <sense>
            <def>sich als Krampus verkleiden</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p5.2">Hausrv.</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c34186"/>
          </sense>
        </sense>
        <re>
          <form subtype="compound" type="lemma">
            <orth>abkrampen</orth>
          </form>
          <sense>
            <sense>
              <def>etwas mit einer Haue entfernen (z. B. Steine oder Früchte von einem Feld oder Mist von einem Karren)</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p3">Stmk.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">Mist åkrampnen</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p2.3c05">St. Georgen am Längsee</placeName>
                </usg>
              </cit>
              <cit type="example">
                <quote xml:lang="bar-AT">die Wiese ågram̄pn</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p3.3c08">Stainz</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c33887"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>ankrampen</orth>
          </form>
          <sense>
            <sense>
              <def>mit einem Rechen auf dem Acker die Abstände der Reihen vorzeichnen, in denen die Feldfrüchte gesetzt werden</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.3">WStmk.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c33889"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>aufkrampen</orth>
          </form>
          <sense>
            <sense>
              <sense>
                <def>auflockern von Erde oder Mist</def>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
                </usg>
                <usg type="geo">
                  <placeName type="bundesland" ref="/geodata-wboe.xml#p3">Stmk.</placeName>
                </usg>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c34187"/>
              </sense>
              <sense>
                <def>aufhängen von Flachs</def>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p6.3">Waldv.</placeName>
                </usg>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c33888"/>
              </sense>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>mistabkrampen</orth>
          </form>
          <sense>
            <sense>
              <sense>
                <def>Mist vom fahrenden Düngerwagen in kleinen, reihenweise geordneten Häufchen mit dem Mistkrampen abladen</def>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
                </usg>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c34188"/>
              </sense>
            </sense>
          </sense>
        </re>
      </entry>
    </body>
