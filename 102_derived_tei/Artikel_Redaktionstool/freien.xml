                    <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.3">öNTir.</placeName>
                  </usg>
                  <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c17777"/>
                </sense>
                <sense>
                  <def>mit der Arbeit fertig</def>
                  <usg type="geo">
                    <placeName type="bundesland" ref="/geodata-wboe.xml#p4">Sbg.</placeName>
                  </usg>
                  <cit type="example">
                    <quote xml:lang="bar-AT">bist pfraid? Aft gengan ma!</quote>
                    <ref type="bibl" target="#SalzburgerWb1995">Salzburger Wb. 1995
                      <citedRange>147</citedRange>
                    </ref>
                    <usg type="geo">
                      <placeName type="bundesland" ref="/geodata-wboe.xml#p4">Sbg.</placeName>
                    </usg>
                  </cit>
                  <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c17773"/>
                </sense>
              </sense>
            </sense>
          </sense>
