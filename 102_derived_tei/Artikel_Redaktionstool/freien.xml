            <orth>befreien</orth>
          </form>
          <sense>
            <sense>
              <sense>
                <def>aus der Gefangenschaft, aus einer bedrängten oder misslichen Lage herausholen und frei machen; erlösen; retten</def>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
                </usg>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c17756"/>
              </sense>
              <sense>
                <xr type="variant">
                  <lbl>nur</lbl>
                  <ref>befreit</ref>
                </xr>
                <usg>(adjektivisches Partizip)</usg>
                <sense>
                  <def>frei; los</def>
                  <usg type="geo">
                    <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.3">öNTir.</placeName>
                  </usg>
                  <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c17776"/>
                </sense>
                <sense>
                  <def>beendet; fertig</def>
