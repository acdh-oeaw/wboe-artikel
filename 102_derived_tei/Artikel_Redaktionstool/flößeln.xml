              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p5">OÖ</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c19380"/>
            </sense>
            <sense>
              <def>„pissen“</def>
              <usg>(jenisch)</usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p2">Ktn.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c19381"/>
            </sense>
          </sense>
          <sense>
            <sense>
              <def type="umschreibung">Ausdruck, wenn ein Mann einer Frau wiederholt vorgeht, um ihr Gesicht zu sehen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c19384"/>
            </sense>
            <sense>
              <def>einer Frau wiederholt auffällig ins Gesicht schauen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c19385"/>
            </sense>
          </sense>
          <sense>
            <def>sich beim Gehen über die Zehen steigen</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p5.1">Innv.</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c19378"/>
          </sense>
          <sense>
