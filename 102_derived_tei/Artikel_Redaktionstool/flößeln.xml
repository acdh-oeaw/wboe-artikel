        <sense>
          <sense>
            <sense>
              <def>fortschleppen; mit Gewalt fortziehen (wie ein Floß, das mit dem Seil fortgezogen wird); mit Gewalt wegbringen</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p2">Ktn.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c19367"/>
            </sense>
            <sense>
              <def>durch Zureden wegbringen</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p2">Ktn.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c19386"/>
