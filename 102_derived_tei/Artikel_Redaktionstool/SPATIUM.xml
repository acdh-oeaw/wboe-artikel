  <text>
    <body>
      <entry>
        <form type="lemma" xml:id="Spatium">
          <orth>Spatium</orth>
        </form>
        <form type="lemma" xml:id="Spatzium">
          <orth>Spatzium</orth>
        </form>
        <form type="lemma" xml:id="Schbadsium">
          <orth>Schbadsium</orth>
        </form>
        <form type="lemma" xml:id="Schpatzjum">
          <orth>Schpatzjum</orth>
        </form>
        <gramGrp></gramGrp>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p7.2">MBgl.</placeName>
        </usg>
        <etym>
          <seg type="desc">Bitte entferne vor dem Punkt das</seg>
          <cit type="etymon">
            <form>
              <orth>Spatium</orth>
            </form>
          </cit>
          <seg type="desc">.</seg>
          <ref type="bibl" subtype="online">Gellan online
            <xr type="lemma">
              <ref>Punkt</ref>
              <date type="accessed">01.06.2023</date>
            </xr>
          </ref>
          <seg type="desc">.</seg>
          <seg type="desc">Früher sagten die Leute ganz gerne mal</seg>
          <cit type="etymon">
            <form>
              <orth>Schbardzium</orth>
            </form>
          </cit>
          <seg type="desc">(</seg>
          <cit type="etymon">
            <form>
              <orth>Leerzeichen</orth>
            </form>
          </cit>
          <seg type="desc">kannte da noch niemand, aber das ist auch lange schon her und da sagten sie auch noch eher</seg>
          <cit type="etymon">
            <form>
              <orth>Wortraum</orth>
            </form>
          </cit>
          <seg type="desc">), wenn ihnen danach war. Also ganz früher (</seg>
          <cit type="etymon"></cit>
          <seg type="desc">
            <date>18. Jahrhundert</date>) mal.
          </seg>
          <seg type="desc">Das Suffix -</seg>
          <cit type="etymon">
            <form>
              <orth>erl</orth>
            </form>
            <def>Diminutiv</def>
          </cit>
          <seg type="desc">und das Präfix</seg>
          <cit type="etymon">
            <form>
              <orth>ver-</orth>
            </form>
          </cit>
          <seg type="desc"></seg>
          <seg type="desc">- sind meine liebsten Fixe.</seg>
          <seg type="desc">Aber natürlich gab's auch noch andere Wörter wie</seg>
          <cit type="etymon">
            <form>
              <orth>Hohlzeichen</orth>
            </form>
            <form>
              <orth>Freigelasse</orth>
            </form>
            <form>
              <orth>Sparplatz</orth>
            </form>
          </cit>
          <seg type="desc">
            <lang>ahd.</lang>
          </seg>
          <cit type="etymon">
            <form>
              <orth>fark</orth>
            </form>
          </cit>
          <cit type="etymon">
            <lang>mhd.</lang>
            <form>
              <orth>fack</orth>
            </form>
          </cit>
        </etym>
        <sense>
          <sense>
            <def>Spatium vor Komma entfernen</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">Da Mo
                <cit type="translation">
                  <quote xml:lang="de">Mann</quote>
                </cit>, der wos do schreibt
              </quote>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c0815"/>
          </sense>
          <sense>
            <xr type="lemma">
              <lbl>nur</lbl>
              <ref>Leerzeichen</ref>
            </xr>
            <usg>;</usg>
            <xr type="lemma">
              <ref>Leerraum</ref>
            </xr>
          </sense>
          <sense></sense>
        </sense>
        <re>
          <form subtype="compound" type="lemma">
            <orth>hottofahren</orth>
          </form>
          <form subtype="compound" type="lemma">
            <orth>hüofahren</orth>
          </form>
          <sense>
            <sense>
              <def></def>
            </sense>
          </sense>
        </re>
      </entry>
