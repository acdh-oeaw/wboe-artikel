              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">a gröpös̄chö Hend</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.3b02">Gmunden</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c44807"/>
            </sense>
            <sense>
