    <body>
      <entry>
        <form type="lemma" xml:id="Gitter">
          <orth>Gitter</orth>
          <ptr type="collection" subtype="lemma" target="/collections-wboe.xml#c29267"/>
        </form>
        <gramGrp>
          <gram type="pos">Substantiv</gram>
          <gram type="gender">Neutrum</gram>
        </gramGrp>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.1">wNTir.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.2">mNTir.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p2.1">ObKtn.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p3">Stmk.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p4.3">Pong.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p4.5">Flachg.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p5">OÖ</placeName>
        </usg>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p6">NÖ</placeName>
        </usg>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p7.1">NBgl.</placeName>
        </usg>
        <note type="kommentar">in Bearbeitung</note>
        <etym>
          <seg type="desc">Offenbar handelt es sich um eine jüngere, seit dem
            <date>15. Jh.</date>belegte Abwandlung zu
          </seg>
          <cit type="etymon">
            <xr type="lemma">
              <ref>Gatter</ref>
            </xr>
          </cit>
          <seg type="desc">. Vielleicht ist</seg>
          <cit type="etymon">
            <form>
              <orth>Gitter</orth>
            </form>
          </cit>
          <seg type="desc">aus einem Kollektivum</seg>
          <cit type="etymon">
            <lang>frnhd.</lang>
            <form>
              <orth>gegitter</orth>
            </form>
          </cit>
          <seg type="desc">(zu</seg>
          <cit type="etymon">
            <lang>mhd.</lang>
            <form>
              <orth>geter</orth>
            </form>
          </cit>
          <seg type="desc">, einer umgelauteten, wohl diminutiven Bildung zu</seg>
          <cit type="etymon">
            <lang>mhd.</lang>
            <form>
              <orth>gater</orth>
            </form>
            <def>Gatter, Gitter</def>
          </cit>
          <seg type="desc">) hervorgegangen.</seg>
          <cit type="etymon"></cit>
          <ref type="bibl" target="#Kluge2011">
            <lbl>vgl.</lbl>Kluge 2011
            <citedRange>360-361</citedRange>
          </ref>
          <ref type="bibl" target="#Pfeifer2014">
            <lbl>vgl.</lbl>Pfeifer 2014
            <citedRange>452</citedRange>
          </ref>
        </etym>
        <sense>
          <sense>
            <sense>
              <def>parallel angeordnete oder gekreuzte, miteinander verbundene Stäbe aus Eisen oder Holz, die als Zaun, Absperrung oder Schutz dienen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1A.4">öSTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.4">Mühlv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c31171"/>
            </sense>
            <sense>
              <def>Einzäunung; Zwinger (für Tiere)</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.4">Weinv.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">a Gitta zan ɒ̊̄špe͡aʳn</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.4d18">Poysdorf</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c31169"/>
            </sense>
            <sense>
              <def>Gefängnis</def>
              <usg>(gaunersprachlich)</usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c31170"/>
            </sense>
          </sense>
        </sense>
        <re>
          <form subtype="compound" type="lemma">
            <orth>Absperrgitter</orth>
          </form>
          <sense>
            <sense>
              <def>Gitter, das dazu dient, den Zugang zu etwas zu versperren</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.1">wNTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.2">Hausrv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.4">Weinv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c31172"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>Bauchgitter</orth>
          </form>
          <sense>
            <sense>
              <def>ausgebauchtes, nach außen gewölbtes Gitter (z. B. vor Fenstern)</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p3">Stmk.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">die an den Häusern angebrachten eisernen B[auchgitter] sind verboten</quote>
                <ref type="bibl" target="#SteirWb1903">Steir.Wb. 1903
                  <citedRange>54</citedRange>
                </ref>
                <usg type="geo">
                  <placeName type="bundesland" ref="/geodata-wboe.xml#p3">Stmk.</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c31185"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>Drahtgitter</orth>
          </form>
          <sense>
            <sense>
              <def>Gitter aus Draht</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.2">Hausrv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.3">Waldv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c31173"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>Fenstergitter</orth>
          </form>
          <sense>
            <sense>
              <def>schmiedeeisernes Gitter vor oder in der Fensteröffnung (als Schutz gegen Einbruch)</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.1">ObKtn.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.2">Hausrv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.2">Industriev.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">eisərnes Fenštərgittər</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p2.1b04">Rangersdorf</placeName>
                </usg>
              </cit>
              <cit type="example">
                <quote xml:lang="bar-AT">éisane Fénstagitta</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.4d18">Poysdorf</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c31175"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>Fliegengitter</orth>
          </form>
          <sense>
            <sense>
              <def>Gitter zum Schutz vor Fliegen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.1">ObKtn.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.2">Industriev.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">Fli͡ag͜ngittăʳ a͡us O͡aʳgantin</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.2j15">Pottendorf</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c31176"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>Kanalgitter</orth>
          </form>
          <sense>
            <sense>
              <def>Gitter über einem Abflussrohr der Kanalisation</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">Hērsd d', i bassí̄ᵃʳ di durch a Kanä́̄uˡgĭtaʳ, daß d' Dauaʳwöˡn schlá̊gsd und bein Donaukanǟuˡ aussikumsd</quote>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c31182"/>
            </sense>
            <sense>
              <def type="umschreibung">Bezeichnung für ein Gebiss mit Zahnlücken; auch abwertende Bezeichnung für den Mund</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c31197"/>
            </sense>
            <sense>
              <def type="umschreibung">Leibesstrafe beim Kinderspiel (Schläge auf die Hand mit einer aus Leder geflochtenen Geißel)</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c31193"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>Sandgitter</orth>
          </form>
          <sense>
            <sense>
              <def type="umschreibung">Bezeichnung für einen aus Baumstämmen gezimmerten Rost, über den das Trift- oder Schwemmholz tragende Wasser fließt, sodass der mitgeführte Schotter abfließen kann</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p5">OÖ</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c31187"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>Seihgitter</orth>
          </form>
          <sense>
            <sense>
              <def>Gitter zum Reinigen des Traubenmosts</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1A.2">mSTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.4">Weinv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c31188"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>Speisgitter</orth>
          </form>
          <sense>
            <sense>
              <def type="umschreibung">Bezeichnung für das Gitter zwischen Chor und Schiff in der Kirche) wo die Kommunion ausgeteilt wird</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.2">mNTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.1">ObKtn.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p3">Stmk.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p4.3">Pong.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p4.5">Flachg.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.1">Innv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.2">Hausrv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c31189"/>
            </sense>
          </sense>
        </re>
        <re type="redewendung">
          <form subtype="MWE" type="lemma">
            <orth>das Kanalgitter halten</orth>
          </form>
          <sense>
            <def>den Mund halten</def>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">s Kanäˡggiddəʳ håiˡdn</quote>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
            </cit>
            <cit type="example">
              <quote xml:lang="bar-AT">Håiˡt ddė̄ⁱ Kanǟˡggĭddəʳ</quote>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c31202"/>
          </sense>
        </re>
