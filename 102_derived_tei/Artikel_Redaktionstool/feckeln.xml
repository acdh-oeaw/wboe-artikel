      <entry>
        <form type="lemma" xml:id="feckeln">
          <orth>feckeln</orth>
          <ptr type="collection" subtype="lemma" target="/collections-wboe.xml#c735"/>
        </form>
        <gramGrp>
          <gram type="pos">Verb</gram>
          <iType>schwach</iType>
        </gramGrp>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p3">Stmk.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p4">Sbg.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p5">OÖ</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p6.2">Industriev.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p6.3">Waldv.</placeName>
        </usg>
        <note type="kommentar" resp="#FF">Für den Stammvokal liegen im gesamten Untersuchungsgebiet
          <c>e</c>-Monophthonge unterschiedlicher Qualität vor
          <xr>
            <ref>ƒękl̥n</ref>
          </xr>. Die Intensität des auslautenden
          <c>k</c>sowie dessen Behauchung unterliegen überall Schwankungen. Aufgrund der Konkurrenzsituation zwischen
          <c>e</c>-Synkope und
          <c>l</c>-Vokalisierung im Mittelbairischen sind in der auslautenden Silbe komplementäre Varianten mit
          <c>l</c>-Erhalt
          <xr>
            <ref>fękl̥n</ref>
          </xr>oder Vokalisierungsprodukten mit unterschiedlicher Qualität
          <xr>
            <ref>fękų̈n</ref>
          </xr>belegt.
        </note>
        <form type="dialect">
          <pron xml:lang="bar-AT">φęgl̥n</pron>
          <usg type="geo">
            <placeName type="grossregion" ref="/geodata-wboe.xml#p5.4">Mühlv.</placeName>
          </usg>
        </form>
        <form type="dialect">
          <pron xml:lang="bar-AT">fękų̈̃n</pron>
          <pron xml:lang="bar-AT">fękl̥n</pron>
          <usg type="geo">
            <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.4e04">Gallneukirchen</placeName>
          </usg>
        </form>
        <form type="dialect">
          <pron xml:lang="bar-AT">-fėkgün</pron>
          <usg type="geo">
            <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.3a15">Kautzen</placeName>
          </usg>
          <ptr type="collection" subtype="lautung" target="/collections-wboe.xml#c738"/>
        </form>
        <etym>
          <seg type="desc">Vermutlich eine abgelautete Form zum Verb</seg>
          <cit type="etymon">
            <xr type="lemma">
              <ref target="ficken.xml">ficken</ref>
            </xr>
            <usg>mit der Bedeutung</usg>
            <form></form>
            <def>rasch hin- und herbewegen</def>
          </cit>
          <seg type="desc">bis hin zu</seg>
          <cit type="etymon">
            <def>herumreiben; kratzen</def>
          </cit>
          <seg type="desc">mit einer durch Nasal verstärkten Stammsilbe</seg>
          <ref type="bibl" target="#SchweizerischesId1881I">
            <lbl>vgl.</lbl>Schweizerisches Id. 1881 I
            <citedRange>715, 866</citedRange>
          </ref>
          <seg type="desc">; siehe auch</seg>
          <cit type="etymon">
            <xr type="lemma">
              <ref target="fienkeln.xml">fienkeln</ref>
            </xr>
          </cit>
        </etym>
        <sense>
          <sense>
            <sense>
              <def>jemanden bei den Haaren oder Ohren reißen</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p3">Stmk.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">bei saini Oawaschl gföklt</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p3.5j25">Unterlamm</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c743"/>
            </sense>
            <sense>
              <def>die Tanzpartnerin ungeschickt hin und her schütteln</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.5">OStmk.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">grissⁿ und gföklt</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p3.5j25">Unterlamm</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c748"/>
            </sense>
          </sense>
          <sense>
            <sense>
              <def>schnitzeln; etwas mit einem Messer in kleine Teile bzw. Stücke zerschneiden</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p4">Sbg.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p5">OÖ</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c749"/>
            </sense>
            <sense>
              <def>kleine Holzspäne abschneiden</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.4">Mühlv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c750"/>
            </sense>
          </sense>
        </sense>
        <re>
          <form subtype="compound" type="lemma">
            <orth>abfeckeln</orth>
            <gramGrp></gramGrp>
          </form>
          <sense>
            <sense>
              <usg>nur</usg>
              <xr type="lemma">
                <ref>abgefeckelt</ref>
              </xr>
              <usg>(adjektivisches Partizip)</usg>
              <def>schlecht abgeschnittenes Brot</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.4">Weinv.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">is Brōd is ogfacklt</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.4j05">Matzen-Raggendorf</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c751"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>umeinanderfeckeln</orth>
          </form>
          <sense>
            <sense>
              <sense>
                <def>an einem Knochen herumnagen</def>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p5.2">Hausrv.</placeName>
                </usg>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p6.2">Industriev.</placeName>
                </usg>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p6.3">Waldv.</placeName>
                </usg>
                <cit type="example">
                  <quote xml:lang="bar-AT">Geh fēgl an den Boa nit a so umanand</quote>
                  <usg type="geo">
                    <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.2a39">Traun</placeName>
                  </usg>
                </cit>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c752"/>
              </sense>
            </sense>
            <sense>
              <sense>
                <def>jemandem mit der Hand oder einer Rute leichte Schläge auf die Beine geben</def>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p6.3">Waldv.</placeName>
                </usg>
                <cit type="example">
                  <quote xml:lang="bar-AT">ąn dė̄ Bǭana umanå̄ndafekaln</quote>
                  <usg type="geo">
                    <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.3a07">Gastern</placeName>
                  </usg>
                </cit>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c753"/>
              </sense>
            </sense>
            <sense>
              <sense>
                <def>einen Geschlechtsakt vollziehen</def>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p6.3">Waldv.</placeName>
                </usg>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c754"/>
              </sense>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>verfeckeln</orth>
          </form>
          <sense>
            <sense>
              <def>etwas durch unrichtiges Schneiden verderben</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.4">Mühlv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c755"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>zerfeckeln</orth>
          </form>
          <sense>
            <sense>
              <sense>
                <def>etwas in kleine Stücke schneiden</def>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p5.4">Mühlv.</placeName>
                </usg>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c756"/>
              </sense>
              <sense>
                <def>etwas durch unrichtiges Schneiden verderben</def>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p5.4">Mühlv.</placeName>
                </usg>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c757"/>
              </sense>
            </sense>
          </sense>
        </re>
      </entry>
