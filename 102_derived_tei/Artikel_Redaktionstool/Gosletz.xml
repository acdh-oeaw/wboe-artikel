          <orth>Koselz</orth>
          <ptr type="collection" subtype="lemma" target="/collections-wboe.xml#c33773"/>
        </form>
        <gramGrp>
          <gram type="pos">Substantiv</gram>
          <gram type="gender">Maskulinum</gram>
          <gram type="gender">Femininum</gram>
        </gramGrp>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p2">Ktn.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p3.1">sbair.ObStmk.</placeName>
        </usg>
        <usg type="geo">
