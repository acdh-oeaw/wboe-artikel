        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p7.1">NBgl.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p7.2">MBgl.</placeName>
        </usg>
        <note type="kommentar"></note>
        <etym>
          <seg type="desc">Seit dem 16. Jh. belegt als ursprünglich vermutlich lautmalendes
            <c>kaudern</c>
          </seg>
          <cit type="etymon">
            <def>wie ein Truthahn kollern, schwatzen</def>
          </cit>
          <seg type="desc">(zu
            <c>Kauder</c>
          </seg>
          <cit type="etymon">
            <def>Truthahn</def>
          </cit>
          <seg type="desc">)
            <ref type="bibl" target="#Kluge2011">
              <lbl>vgl.</lbl>Kluge 2011
              <citedRange>482</citedRange>
            </ref>
          </seg>
          <seg type="desc"></seg>
        </etym>
        <sense>
          <sense>
            <def>kollern (vom Truthahn)</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40842"/>
          </sense>
          <sense>
            <def>lachen</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40843"/>
          </sense>
          <sense>
            <sense>
              <def>reden; plaudern; tratschen; schwätzen (auch abwertend)</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1B">OTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.4">Weinv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p7.1">NBgl.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p7.2">MBgl.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40840"/>
            </sense>
            <sense>
              <def>plappern; viel und gern reden</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.3">Traunv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.3">Waldv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40835"/>
            </sense>
            <sense>
              <def>überhastet reden und deshalb stottern</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.3">Waldv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40844"/>
            </sense>
            <sense>
              <def>unverständlich oder verworren reden</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p3">Stmk.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.3">Waldv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p7.1">NBgl.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40838"/>
            </sense>
          </sense>
          <sense>
            <def>hausieren; handeln</def>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p1B">OTir.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40794"/>
          </sense>
          <sense>
            <sense>
              <def>auf besondere Art kauen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.3">Traunv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40834"/>
            </sense>
            <sense>
              <def>unappetitlich essen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.3">Traunv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40833"/>
            </sense>
          </sense>
        </sense>
        <re>
          <form subtype="compound" type="lemma">
            <orth>ausherkaudern</orth>
          </form>
          <sense>
            <sense>
              <def>aus dem Mund lassen; speien</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.3">Traunv.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">khaudərts w̄īdəʳ au-a</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.3b02">Gmunden</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40795"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>fürkaudern</orth>
          </form>
          <sense>
            <sense>
              <def>jemandem etwas vorschwätzen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p7.1">NBgl.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40796"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>zusammenkaudern</orth>
          </form>
          <sense>
            <sense>
              <def>schwatzen; (sich etwas) zusammenplappern</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.4">Weinv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">sí̄ wōs zó̄mkaudan</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.4b03">Haugsdorf</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40793"/>
            </sense>
            <sense>
              <def>dummes Zeug zusammenschwatzen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40797"/>
            </sense>
          </sense>
        </re>
