      <entry>
        <form type="lemma" xml:id="gliedeln">
          <orth>gliedeln</orth>
          <ptr type="collection" subtype="lemma" target="/collections-wboe.xml#c31691"/>
        </form>
        <gramGrp>
          <gram type="pos">Verb</gram>
          <iType>schwach</iType>
        </gramGrp>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.3">öNTir.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p5.2">Hausrv.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p5.3">Traunv.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p5.4">Mühlv.</placeName>
        </usg>
        <note type="kommentar">folgt</note>
        <etym>
          <seg type="desc">Ableitung von</seg>
          <cit type="etymon">
            <xr type="lemma">
              <ref target="Glied.xml#Glied">Glied</ref>
            </xr>
          </cit>
        </etym>
        <sense>
          <sense>
            <def>gelenkig sein beim Gehen</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.3">öNTir.</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c36745"/>
          </sense>
          <sense>
            <def>ein Gelenk ausrenken</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p5.4">Mühlv.</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c36776"/>
          </sense>
        </sense>
        <re>
          <form subtype="compound" type="lemma">
            <orth>abgliedeln</orth>
          </form>
          <sense>
            <sense>
              <def>ein Ketten- oder Körperglied vom (organischen) Ganzen wegnehmen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c36747"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>ausgliedeln</orth>
          </form>
          <sense>
            <sense>
              <def>ein Gelenk aus- oder verrenken</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.3">öNTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.2">Hausrv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.3">Traunv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.4">Mühlv.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">in F[inger] ausgliedln</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.2a39">Traun</placeName>
                </usg>
              </cit>
              <cit type="example">
                <quote xml:lang="bar-AT">I håb ma wås ausgliedlt</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.2a39">Traun</placeName>
                </usg>
              </cit>
              <cit type="example">
                <quote xml:lang="bar-AT">Boan ausgliedln</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.2b18">Lambach</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c36748"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>eingliedeln</orth>
          </form>
          <sense>
            <sense>
              <def>gebrochene Kettenglieder wieder zusammenfügen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.3">öNTir.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c36749"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>zusammengliedeln</orth>
          </form>
          <sense>
            <sense>
              <def>zwei oder mehrere Glieder miteinander verbinden, verketten</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c36746"/>
            </sense>
          </sense>
        </re>
