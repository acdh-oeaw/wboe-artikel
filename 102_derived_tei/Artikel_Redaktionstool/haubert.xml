                  <placeName type="grossregion" ref="/geodata-wboe.xml#p5.1">Innv.</placeName>
                </usg>
                <cit type="example">
                  <quote xml:lang="bar-AT">gąns khāuwɒd is ɒ</quote>
                  <usg type="geo">
                    <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.1c07">Mühlheim am Inn</placeName>
                  </usg>
                </cit>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c45766"/>
