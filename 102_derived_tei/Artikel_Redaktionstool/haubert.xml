                  <placeName type="bundesland" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
                </usg>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c45761"/>
              </sense>
              <sense>
