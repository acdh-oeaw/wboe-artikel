                <placeName type="gemeinde" ref="/geodata-wboe.xml#p1C.3f02">Hopfgarten im Brixental</placeName>
              </usg>
            </cit>
            <cit type="example">
              <quote xml:lang="bar-AT">gekåchtet håt'r</quote>
              <ref type="bibl" target="#Hupfauf1956">Hupfauf 1956
                <citedRange>42</citedRange>
              </ref>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.2">mNTir.</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c37657"/>
          </sense>
