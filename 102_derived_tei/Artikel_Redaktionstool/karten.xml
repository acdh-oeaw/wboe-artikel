            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p4">Sbg.</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">dö Fastnpfeiste tuet man ben ins be de Bäck und ben an Toal Wirt, Bretzn kārtn</quote>
