              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.4">Mühlv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">τu κao̩στɒrɒ τu</quote>
                <ref type="bibl" target="#Neuner1966">Neuner 1966
                  <citedRange>260</citedRange>
                </ref>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.2b44">Ungenach</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c33440"/>
            </sense>
            <sense>
              <def>nicht ernst zu nehmender Mensch</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.2">mbair.ObStmk.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c33443"/>
            </sense>
          </sense>
          <sense>
