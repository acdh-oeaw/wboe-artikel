            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p5">OÖ</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c33441"/>
          </sense>
