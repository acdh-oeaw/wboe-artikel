        <form type="lemma" xml:id="Klamsch">
          <orth>Klamsch</orth>
          <ptr type="collection" subtype="lemma" target="/collections-wboe.xml#c44204"/>
        </form>
        <gramGrp>
          <gram type="pos">Substantiv</gram>
          <gram type="gender">Maskulinum</gram>
        </gramGrp>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p3.2">mbair.ObStmk.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p6.3">Waldv.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p6.4">Weinv.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
        </usg>
        <note type="kommentar"></note>
        <etym>
          <seg type="desc">Unklar; es könnte sich um eine Weiterentwicklung zu</seg>
          <cit type="etymon">
            <lang>frnhd.</lang>
            <form>
              <orth>klam</orth>
            </form>
            <def>Not; Mangel; körperliche Beklemmung</def>
          </cit>
          <seg type="desc">handeln und daher etymologisch zu den Verben</seg>
          <cit type="etymon">
            <lang>nhd.</lang>
            <form>
              <orth>klemmen</orth>
            </form>
          </cit>
          <cit type="etymon">
            <form>
              <orth>klimmen</orth>
            </form>
          </cit>
          <seg type="desc">zu stellen sein
            <ref type="bibl" subtype="online">Pfeifer online
              <xr type="lemma">
                <ref>klimmen; klemmen</ref>
                <date type="accessed">17.02.2025</date>
              </xr>
            </ref>
          </seg>
          <seg type="desc">. Alternativ kann es auf</seg>
          <cit type="etymon">
            <lang>tschech.</lang>
            <form>
              <orth>klam</orth>
            </form>
            <def>Täuschung; falsche Vorstellung; Fehler</def>
          </cit>
          <cit type="etymon">
            <form>
              <orth>klamat</orth>
            </form>
            <def>täuschen; irreführen</def>
          </cit>
          <seg type="desc">zurückgeführt werden, wobei keine von
            <c>klam-</c>abgeleitete Form auf
            <c>-sch</c>
            <lang>bekannt ist und eine Weiterentwicklung zu</lang>
          </seg>
          <cit type="etymon">
            <lang>tschech.</lang>
            <form>
              <orth>klamač</orth>
            </form>
            <def>Betrüger</def>
          </cit>
          <seg type="desc">unwahrscheinlich ist
            <ref type="bibl" target="#WrWb2002">
              <lbl>vgl.</lbl>WrWb. 2002
              <citedRange>418</citedRange>
            </ref>
          </seg>
          <seg type="desc">. Die frühesten Zeitungsbelege, die
            <date>Ende des 19. Jahrhunderts</date>auftreten, stammen aus Texten, in denen Sprechen mit jiddischem Akzent imitiert wird
            <ref type="bibl" target="#Kikieriki">
              <lbl>vgl.</lbl>Kikeriki
              <citedRange>23.01.1898: 3</citedRange>
            </ref>
          </seg>
          <seg type="desc">, und lassen daher auch an jiddischen Ursprung oder jiddische Vermittlung denken.</seg>
        </etym>
        <sense>
          <sense>
            <sense>
              <def>geistiger Defekt; Hieb</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c44211"/>
            </sense>
            <sense>
              <def>(Alkohol-)Rausch; Schwips</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.2">mbair.ObStmk.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">der håt gestern an urndlichen Klåmsch g'håbt!</quote>
                <usg type="geo">
                  <placeName type="bundesland" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c44210"/>
            </sense>
          </sense>
        </sense>
