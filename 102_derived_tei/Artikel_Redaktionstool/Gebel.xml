            <placeName type="grossregion" ref="/geodata-wboe.xml#p3.2">mbair.ObStmk.</placeName>
          </usg>
          <cit type="example">
            <quote xml:lang="bar-AT">Gib eam an's auf sein Gepəl</quote>
            <usg type="geo">
              <placeName type="gemeinde" ref="/geodata-wboe.xml#p3.2a07">Proleb</placeName>
            </usg>
          </cit>
          <cit type="example">
            <quote xml:lang="bar-AT">I hau daʳ glai oans aufi auf dain Gė̄wl</quote>
            <usg type="geo">
              <placeName type="gemeinde" ref="/geodata-wboe.xml#p3.2j10">Schladming</placeName>
            </usg>
          </cit>
          <ptr type="collection" subtype="MWE"/>
        </re>
        <re type="redewendung">
          <xr type="variant" subtype="diminutive">
            <ref>Gebelein</ref>
          </xr>
          <form subtype="MWE" type="lemma">
            <orth>ein Gebelein</orth>
          </form>
