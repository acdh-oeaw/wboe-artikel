            <def>eine Handvoll</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p2.4">UKtn.</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">ã gépale kraut</quote>
              <usg type="geo">
                <placeName type="kleinregion" ref="/geodata-wboe.xml#p2.4d">mLavantt.</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung"/>
          </sense>
        </re>
