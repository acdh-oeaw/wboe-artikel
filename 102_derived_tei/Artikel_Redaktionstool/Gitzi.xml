              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.3s04">Langenlois</placeName>
              </usg>
            </cit>
            <cit type="example">
              <quote xml:lang="bar-AT">Hå̄sd ăn Gīdsi hå̄sd ăn Gagg</quote>
              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.5w06">Mauer</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c36599"/>
          </sense>
