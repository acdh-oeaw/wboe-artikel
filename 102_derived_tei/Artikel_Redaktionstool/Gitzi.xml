            </usg>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p6.3">Waldv.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">hait håt sė an Khīzė</quote>
              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p2.3n01">Klagenfurt</placeName>
              </usg>
            </cit>
            <cit type="example">
              <quote xml:lang="bar-AT">einen gīdsė haben</quote>
              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p3.4d01">Graz</placeName>
              </usg>
            </cit>
            <cit type="example">
              <quote xml:lang="bar-AT">hosd ɒn gids?</quote>
              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.1o01">Herzogenburg</placeName>
              </usg>
            </cit>
            <cit type="example">
