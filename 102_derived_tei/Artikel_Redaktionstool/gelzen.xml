        <note type="kommentar"></note>
        <etym>
          <seg type="desc">
            <lang>mhd.</lang>
          </seg>
          <cit type="etymon">
            <form>
              <orth>*gëlzen</orth>
            </form>
          </cit>
          <seg type="desc">, vergleiche</seg>
          <cit type="etymon">
            <form>
              <orth>ergëlzen</orth>
            </form>
            <def>die Stimme erschallen lassen</def>
          </cit>
          <ref type="bibl" target="#Lexer1872I">
            <lbl>vgl.</lbl>Lexer 1872 I
            <citedRange>832, 629</citedRange>
          </ref>
          <ref type="bibl" target="#BMZ1854I">
            <lbl>vgl.</lbl>BMZ 1854 I
            <citedRange>519b</citedRange>
          </ref>
          <seg type="desc">, wie</seg>
          <cit type="etymon">
            <form>
              <orth>gëlsen</orth>
            </form>
            <def>gellen; schreien; heulen</def>
          </cit>
          <seg type="desc">von</seg>
          <cit type="etymon">
            <form>
              <orth>gëllen</orth>
            </form>
            <def>laut tönen; schreien</def>
          </cit>
          <seg type="desc">gebildet</seg>
          <ref type="bibl" target="#Lexer1872I">
            <lbl>vgl.</lbl>Lexer 1872 I
            <citedRange>825</citedRange>
          </ref>
          <seg type="desc">; siehe auch</seg>
          <cit type="etymon">
            <xr type="lemma">
              <ref target="gelsen.xml">gelsen</ref>
            </xr>
          </cit>
          <seg type="desc">. Im Gegensatz zu letzterem stehen bei der großen Mehrheit der in der Datenbank zum WBÖ belegten Bedeutungen von</seg>
          <cit type="etymon">
            <form>
              <orth>gelzen</orth>
            </form>
          </cit>
          <seg type="desc">(und dem nur ein Mal belegten</seg>
          <cit type="etymon">
            <form>
              <orth>gilzen</orth>
            </form>
          </cit>
          <seg type="desc">) samt Wortbildungen dazu die Bewegungen im Vordergrund, die mit dem (lauten) Tönen oder dem Schall verbunden sind bzw. das betreffende Geräusch verursachen.</seg>
        </etym>
        <sense>
          <sense>
            <sense>
              <def>schreien; heulen</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1B">OTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c36045"/>
            </sense>
            <sense>
              <def>quieken (vom Ferkel)</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.1">ObKtn.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c36046"/>
            </sense>
          </sense>
          <sense>
            <sense>
              <sense>
                <def>abspringen; abschnellen (von einer Kugel)</def>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.3">öNTir.</placeName>
                </usg>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p4.3">Pong.</placeName>
                </usg>
                <cit type="example">
                  <quote xml:lang="bar-AT">Af a Stoaⁿplătt is dö Khugl gölzt</quote>
                  <usg type="geo">
                    <placeName type="gemeinde" ref="/geodata-wboe.xml#p1C.3f02">Hopfgarten im Brixental</placeName>
                  </usg>
                </cit>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c33750"/>
              </sense>
              <sense>
                <xr type="lemma">
                  <lbl>nur</lbl>
                  <ref>sich gelzen</ref>
                </xr>
                <usg>(reflexiv)</usg>
                <def>hochspringen und sich hoch durch die Luft bewegen</def>
                <usg type="geo">
                  <placeName type="bundesland" ref="/geodata-wboe.xml#p1B">OTir.</placeName>
                </usg>
                <cit type="example">
                  <quote xml:lang="bar-AT">der bâm gölz't si</quote>
                  <usg type="geo">
                    <placeName type="kleinregion" ref="/geodata-wboe.xml#p1B.0d">Def.</placeName>
                  </usg>
                </cit>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c33741"/>
              </sense>
            </sense>
            <sense>
              <def>rutschen; ausrutschen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.2">mNTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.3">öNTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.3">Traunv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c33709"/>
            </sense>
            <sense>
              <def>ungeschickt und schwerfällig gehen (von Menschen)</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.3">öNTir.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c33701"/>
            </sense>
            <sense>
              <def>rennen; laufen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.3">öNTir.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c33698"/>
              <sense>
                <def>ungeschickt laufen (von Menschen)</def>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.3">öNTir.</placeName>
                </usg>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c33703"/>
              </sense>
              <sense>
                <def>übermütig laufen, rennen (von Tieren)</def>
                <usg type="geo">
