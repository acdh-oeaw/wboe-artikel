              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p1C.3e06">Kufstein</placeName>
              </usg>
            </cit>
            <cit type="example">
              <quote xml:lang="bar-AT">åber fix</quote>
              <ref type="bibl" target="#WrWb1929">WrWb. 1929
                <citedRange>60</citedRange>
              </ref>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c23080"/>
          </sense>
        </sense>
        <re>
