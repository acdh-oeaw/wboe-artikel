          <cit type="etymon">
            <xr type="lemma">
              <ref target="frei.xml">frei</ref>
            </xr>
          </cit>
        </etym>
        <sense>
          <sense>
            <sense>
              <def>Freistätte, wo arme Leute ihre Tiere grasen lassen können</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.4">Weinv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c12487"/>
            </sense>
            <sense>
              <def>gesicherter Grundbesitz eines Dorfes oder Hofes</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.4">Weinv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c12488"/>
            </sense>
          </sense>
          <sense>
            <sense>
              <def>Holzgebilde, das eine Hand darstellt, die ein Schwert aufrecht hält, als Zeichen der kaiserlichen Markterlaubnis</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.1">sbair.ObStmk.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c12462"/>
            </sense>
          </sense>
        </sense>
        <re>
