    <body>
      <entry>
        <form type="lemma" xml:id="Kiberer">
          <orth>Kiberer</orth>
          <ptr type="collection" subtype="lemma" target="/collections-wboe.xml#c38668"/>
        </form>
        <gramGrp>
          <gram type="pos">Substantiv</gram>
          <gram type="gender">Maskulinum</gram>
        </gramGrp>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p4">Sbg.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
        </usg>
        <note type="kommentar"></note>
        <etym>
          <seg type="desc">Die genaue Herkunft des Lexems ist nicht eindeutig geklärt. Das Wort stammt aus dem Rotw. und ist vermutlich zu</seg>
          <cit type="etymon">
            <lang>jidd.</lang>
            <form>
              <orth>koiwesch/kobesch sein</orth>
            </form>
            <def>bezwingen; bedrücken</def>
          </cit>
          <seg type="desc">oder</seg>
          <cit type="etymon">
            <lang>jidd.</lang>
            <form>
              <orth>kewius</orth>
            </form>
            <def>Gewissheit; Sicherheit</def>
          </cit>
          <seg type="desc">oder zu</seg>
          <cit type="etymon">
            <lang>mhd.</lang>
            <form>
              <orth>kiben; kabelen</orth>
            </form>
            <def>schimpfen</def>
          </cit>
          <seg type="desc">gebildet.</seg>
          <ref type="bibl" target="#Siewert2023">
            <lbl>vgl.</lbl>Siewert 2023
            <citedRange>393</citedRange>
          </ref>
          <seg type="desc">In Wien ist für das Jahr
            <date>1922</date>der Begriff
          </seg>
          <cit type="etymon">
            <form>
              <orth>Kuberer</orth>
            </form>
            <def>Sittenpolizeikommissar</def>
          </cit>
          <seg type="desc">belegt, der wiederum auf</seg>
          <cit type="etymon">
            <lang>jidd.</lang>
            <form>
              <orth>Kübbo, Kübbe</orth>
            </form>
            <def>Schlafkammer; Bordell; Hurenhaus</def>
          </cit>
          <seg type="desc">zurückgeführt wird.</seg>
          <cit type="etymon">
            <ref type="bibl" target="#WrWb2002">
              <lbl>vgl.</lbl>WrWb. 2002
              <citedRange>545</citedRange>
            </ref>
          </cit>
        </etym>
        <sense>
          <sense>
            <sense>
              <def>Kriminalpolizist; Kriminalbeamter</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c38669"/>
            </sense>
            <sense>
              <def>Geheimpolizist</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p4">Sbg.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c38670"/>
            </sense>
          </sense>
        </sense>
      </entry>
