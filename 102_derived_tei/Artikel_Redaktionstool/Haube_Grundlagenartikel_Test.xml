    <body>
      <entry>
        <form type="lemma" xml:id="Haube">
          <orth>Haube</orth>
          <ptr type="collection" subtype="lemma" target="/collections-wboe.xml#c45755"/>
        </form>
        <gramGrp>
          <gram type="pos">Substantiv</gram>
          <gram type="gender">Femininum</gram>
        </gramGrp>
        <note type="kommentar"></note>
		<sense>
            <def>Kopfbedeckung; Aufsatz, Überzeugung; Netzmagen des Rindes; alte Geschichte</def>
        </sense>
        <etym>
          <cit type="etymon">
            <lang>ahd.</lang>
            <form>
              <orth>hûba</orth>
            </form>
            <def>Haube; Hut; Mütze; Mitra; Tiara</def>
          </cit>
          <cit type="etymon">
            <lang>mhd.</lang>
            <form>
              <orth>hûbe</orth>
            </form>
            <def>Kopfbedeckung; Helm; Federbusch am Kopf von Vögeln</def>
          </cit>
          <cit type="etymon">
            <lang>frnhd.</lang>
            <form>
              <orth>haube</orth>
            </form>
            <def>Kopfbedeckung; Helm; Hülse; Kopfhülle; Federbusch am Kopf von Vögeln</def>
          </cit>
          <ref type="bibl" subtype="online">
            <lbl>vgl.</lbl>AWB online
            <xr type="lemma">
              <ref>hûba</ref>
              <date type="accessed">25.06.2025</date>
            </xr>
          </ref>
          <ref type="bibl" subtype="online">Mhd. Wb. online
            <xr type="lemma">
              <ref>hûbe</ref>
              <date type="accessed">25.06.2025</date>
            </xr>
          </ref>
          <ref type="bibl" subtype="online">Frnhd. Wb. online
            <xr type="lemma">
              <ref>haube</ref>
              <date type="accessed">25.06.2025</date>
            </xr>
          </ref>
          <seg type="desc">; Als
            <lang>urgermanischer</lang>Stamm des Wortes wird
          </seg>
          <cit type="etymon">
            <form>
              <orth>*χūƀōn-</orth>
            </form>
            <def>die nach außen, oben Gewölbte</def>
          </cit>
          <seg type="desc">rekonstruiert, bezüglich der zugrundeliegenden
            <lang>indogermanischen</lang>Wurzel besteht Uneinigkeit: Entweder geht
          </seg>
          <cit type="etymon">
            <form>
              <orth>Haube</orth>
            </form>
          </cit>
          <seg type="desc">auf</seg>
          <cit type="etymon">
            <lang>uridg.</lang>
            <form>
              <orth>*keu̯bh-</orth>
            </form>
            <def>sich beugen, ducken</def>
          </cit>
          <seg type="desc">oder auf</seg>
          <cit type="etymon">
            <lang>uridg.</lang>
            <form>
              <orth>*kuHbʰ-/*kuHp-</orth>
            </form>
            <def>sich wölben / Gewölbtes</def>
          </cit>
          <seg type="desc">zurück. Manche Autoren nehmen sogar ein
            <lang>nicht-indoeuropäisches, alteuropäisches</lang>Wort als Etymon an
          </seg>
          <seg type="desc">
            <ref type="bibl" subtype="online">
              <lbl>vgl.</lbl>EWA online
              <xr type="lemma">
                <ref>hûba</ref>
                <date type="accessed">25.06.2025</date>
              </xr>
            </ref>.
          </seg>
        </etym>
      </entry>
