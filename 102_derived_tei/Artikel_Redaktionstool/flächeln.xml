              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c6772"/>
            </sense>
            <sense>
              <def>die Arme beim Gehen hin und her schwingen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.2">Industriev.</placeName>
              </usg>
              <cit type="example">
