      <entry>
        <form type="lemma" xml:id="flaecheln">
          <orth>flächeln</orth>
          <ptr type="collection" subtype="lemma" target="/collections-wboe.xml#c2545"/>
        </form>
        <gramGrp>
          <gram type="pos">Verb</gram>
          <iType>schwach</iType>
        </gramGrp>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p4">Sbg.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p5">OÖ</placeName>
        </usg>
        <usg type="geo">
