            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.1">wNTir.</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">Voll Gutzl sein</quote>
              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p1A.1a02">Mals</placeName>
              </usg>
            </cit>
            <cit type="example">
              <quote xml:lang="bar-AT">Håsch ban ɔ̄rsch ā Gutzl?</quote>
              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p1A.1a02">Mals</placeName>
              </usg>
            </cit>
            <cit type="example">
              <quote xml:lang="bar-AT">Gutzl in t' Nas</quote>
