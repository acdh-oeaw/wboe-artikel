                <placeName type="gemeinde" ref="/geodata-wboe.xml#p1A.1h04">Meran</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c44612"/>
          </sense>
        </sense>
