                <placeName type="gemeinde" ref="/geodata-wboe.xml#p1A.1a02">Mals</placeName>
              </usg>
            </cit>
            <cit type="example">
              <quote xml:lang="bar-AT">I hǫnn di Gutzl untrn ɔ̨rm</quote>
              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p1A.1d03">Schlanders</placeName>
              </usg>
            </cit>
            <cit type="example">
              <quote xml:lang="bar-AT">dear håt di gutzl</quote>
              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p1A.1h04">Meran</placeName>
