          <pron xml:lang="bar-AT">fǫəflə</pron>
          <pron xml:lang="bar-AT">fǫɒ̄flɒ</pron>
          <usg type="geo">
            <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.1c07">Mühlheim am Inn</placeName>
          </usg>
          <ptr type="collection" subtype="lautung" target="/collections-wboe.xml#c13738"/>
        </form>
        <etym>
          <seg type="desc">Ableitung von</seg>
          <cit type="etymon">
            <xr type="lemma">
              <ref target="farfeln.xml#farfeln">farfeln</ref>
            </xr>
          </cit>
        </etym>
        <sense>
          <sense>
            <sense>
              <def>Schwätzer; Person, die gerne und viel redet</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.2">mbair.ObStmk.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.1">Innv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c13741"/>
            </sense>
            <sense>
              <def>Person, die schnell, undeutlich, unverständlich redet</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.1">Innv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c13742"/>
            </sense>
            <sense>
              <def>Person, die dumm oder übertrieben daherredet</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p4.1">Lung.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.1">Innv.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">dös is a Fårfla</quote>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p4.1">Lung.</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c13740"/>
            </sense>
          </sense>
        </sense>
