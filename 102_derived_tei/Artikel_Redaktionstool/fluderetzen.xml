              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c3639"/>
            </sense>
            <sense>
              <def>mühsam flattern</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.4">Weinv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c3642"/>
            </sense>
          </sense>
          <sense>
            <sense>
