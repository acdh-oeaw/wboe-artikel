          <usg type="geo">
            <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.4d18">Poysdorf</placeName>
          </usg>
          <ptr type="collection" subtype="lautung" target="/collections-wboe.xml#c3621"/>
        </form>
        <etym>
