        <etym>
          <seg type="desc">entlehnt aus
            <lang>frz.</lang>
          </seg>
          <cit type="etymon">
            <form>
              <orth>flacon</orth>
            </form>
            <def>Flakon; Fläschchen</def>
          </cit>
          <seg type="desc">; Im Bairischen in Südtirol und Österreich ist vermehrt ein Wechsel von</seg>
          <cit type="etymon">
            <form>
              <orth>l</orth>
            </form>
          </cit>
          <seg type="desc">zu</seg>
          <cit type="etymon">
            <form>
              <orth>r</orth>
            </form>
          </cit>
          <seg type="desc">eingetreten</seg>
          <ref type="bibl" target="#KärntischesWb1862">
            <lbl>vgl.</lbl>Kärntisches Wb. 1862
            <citedRange>101</citedRange>
          </ref>
          <seg type="desc">, besagtes</seg>
          <cit type="etymon">
            <form>
              <orth>l</orth>
            </form>
          </cit>
          <seg type="desc">ist in anderen Teilen des Bairischen noch erhalten geblieben
            <ref type="bibl" target="#Schmeller1872I">
              <lbl>vgl.</lbl>Schmeller 1872 I
              <citedRange>786, 806</citedRange>
            </ref>
          </seg>
          <seg type="desc">. Der Wechsel von</seg>
          <cit type="etymon">
            <form>
              <orth>r</orth>
            </form>
          </cit>
          <seg type="desc">zu</seg>
          <cit type="etymon">
            <form>
              <orth>l</orth>
            </form>
          </cit>
          <seg type="desc">ist vermutlich auf einen Dissimilationsprozess mit den Diminutiv-Suffixen -</seg>
          <cit type="etymon">
            <form>
              <orth>el</orth>
            </form>
          </cit>
          <seg type="desc">, -</seg>
          <cit type="etymon">
            <form>
              <orth>erl</orth>
            </form>
          </cit>
          <seg type="desc">und -</seg>
          <cit type="etymon">
            <form>
              <orth>elein</orth>
            </form>
          </cit>
          <seg type="desc">zurückzuführen. Die Grundform</seg>
          <cit type="etymon">
            <form>
              <orth>Frack</orth>
            </form>
          </cit>
          <seg type="desc">ist demnach eine Rückbildung aus einer urspünglicheren Diminutivform.</seg>
        </etym>
