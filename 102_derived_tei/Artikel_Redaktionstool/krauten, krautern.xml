        <sense>
          <sense>
            <sense>
              <def>unverständlich sprechen</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c39807"/>
            </sense>
            <sense>
              <def>schimpfen; schelten; toben; zornig sein</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.1">wNTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.2">mNTir.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">brummlen und krauten</quote>
                <usg type="geo">
                  <placeName type="kleinregion" ref="/geodata-wboe.xml#p1C.1t">uÖtzt.</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c39808"/>
            </sense>
          </sense>
          <sense>
            <sense>
              <def>kommen</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p2">Ktn.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">dər dėκhl κhrautət
                  <cit type="translation">
                    <quote xml:lang="de">der Gendarm kommt</quote>
                  </cit>
                </quote>
                <usg type="geo">
                  <placeName type="bundesland" ref="/geodata-wboe.xml#p2">Ktn.</placeName>
                </usg>
              </cit>
              <cit type="example">
                <quote xml:lang="bar-AT">dər flōsl κhrautət
                  <cit type="translation">
                    <quote xml:lang="de">der Regen kommt</quote>
                  </cit>
                </quote>
                <usg type="geo">
                  <placeName type="bundesland" ref="/geodata-wboe.xml#p2">Ktn.</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c39809"/>
            </sense>
            <sense>
              <def>mit wenig Mitteln, langsam arbeiten; nebensächliche Arbeiten verrichten</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.4">MStmk.</placeName>
              </usg>
