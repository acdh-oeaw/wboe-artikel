  <text>
    <body>
      <entry>
        <form type="lemma" xml:id="Galerie">
          <orth>Galerie</orth>
          <ptr type="collection" subtype="lemma" target="/collections-wboe.xml#c26140"/>
        </form>
        <gramGrp>
          <gram type="pos">Substantiv</gram>
          <gram type="gender">Femininum</gram>
        </gramGrp>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p6.4">Weinv.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
        </usg>
        <etym>
          <seg type="desc">
            <date>Ende des 16. Jahrhunderts</date>entlehnt aus
          </seg>
          <cit type="etymon">
            <lang>ital.</lang>
            <form>
              <orth>galleria</orth>
            </form>
          </cit>
          <seg type="desc">oder</seg>
          <cit type="etymon">
            <lang>frz.</lang>
            <form>
              <orth>galerie</orth>
            </form>
            <def>langer überdachter Gang</def>
          </cit>
          <seg type="desc">; zunächst als Fachwort der Architektur</seg>
          <cit type="etymon">
            <def>Säulengang; Balkon; Empore</def>
          </cit>
          <seg type="desc">, der Gartenbaukunst</seg>
          <cit type="etymon">
            <def>Laubengang</def>
          </cit>
          <seg type="desc">und des Festungsbaus</seg>
          <cit type="etymon">
            <def>überdeckter Graben</def>
          </cit>
          <seg type="desc">belegt, ab dem
            <date>17. Jahrhundert</date>, wie im
            <lang>Italienischen</lang>, in der Bedeutung
          </seg>
          <cit type="etymon">
            <def>Bilder-, Gemäldesammlung</def>
          </cit>
          <seg type="desc">und ab dem
            <date>18. Jh.</date>in der Bedeutung
          </seg>
          <cit type="etymon">
            <def>oberster Rang im Theater</def>
          </cit>
          <seg type="desc">verwendet
            <ref type="bibl" subtype="online">
              <lbl>vgl.</lbl>Pfeifer online
              <xr type="lemma">
                <ref>Galerie</ref>
                <date type="accessed">21.04.2023</date>
              </xr>
            </ref>
          </seg>
        </etym>
        <sense>
          <sense>
            <def>Ausstellungssaal voller Kunstwerke</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">wia r a gaunze galarii fola r eiszopfm</quote>
              <ref type="bibl" target="#Artmann1958">Artmann 1958
                <citedRange>64</citedRange>
              </ref>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c35193"/>
          </sense>
          <sense>
            <def>Verbrecherwelt; Wiener Unterwelt; Diebesbande</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">anständige Gäste? Eine Bagasch verkehrt da, ein G'lumpert, eine Galerie</quote>
              <ref type="bibl" target="#Klauser1986">Klauser 1986
                <citedRange>281</citedRange>
              </ref>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung"/>
          </sense>
        </sense>
        <re>
          <form subtype="compound" type="lemma">
            <orth>Matthieselgalerie</orth>
          </form>
          <sense>
            <sense>
              <def>letzter/höchster Rang in einem Theater, für den der geringste Eintrittspreis bezahlt werden muss</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c35190"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>Schmalspurgalerie</orth>
          </form>
          <sense>
            <sense>
              <def>Gesindel</def>
              <usg>(gaunersprachlich)</usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c35191"/>
            </sense>
          </sense>
        </re>
      </entry>
