      <entry>
        <form type="lemma" xml:id="fiesko">
          <orth>fiesko</orth>
          <ptr type="collection" subtype="lemma" target="/collections-wboe.xml#c5438"/>
        </form>
        <gramGrp>
          <gram type="pos">Adjektiv</gram>
        </gramGrp>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p6.3">Waldv.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p6.4">Weinv.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
        </usg>
        <note type="kommentar" resp="#FF">Die ursprünglichen Betonungsverhältnisse des Italienischen wurden unverändert beibehalten, weswegen überall eine hiatische und keine diphthongische Aussprache vorliegt
          <xr>
            <ref>fieskǫ</ref>
          </xr>
        </note>
        <form type="dialect">
          <pron xml:lang="bar-AT">fiésko</pron>
          <usg type="geo">
            <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.1s08">Zeiselmauer-Wolfpassing</placeName>
          </usg>
        </form>
        <form type="dialect">
          <pron xml:lang="bar-AT">fiéskgo</pron>
          <usg type="geo">
            <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.3a15">Kautzen</placeName>
          </usg>
        </form>
        <form type="dialect">
          <pron xml:lang="bar-AT">fiēsko</pron>
          <usg type="geo">
            <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.5j01">Favoriten</placeName>
          </usg>
          <ptr type="collection" subtype="lautung" target="/collections-wboe.xml#c5440"/>
        </form>
        <etym>
          <seg type="desc">Da eine kurzgeschorene Frisur</seg>
          <cit type="etymon">
            <form>
              <orth>a la Fieschi</orth>
            </form>
          </cit>
          <seg type="desc">
            <date>um ca. 1840</date>in Wien in Mode kam, wahrscheinlich nach Joseph Fieschi (
          </seg>
          <seg type="desc">
            <date>1790-1836</date>), der
          </seg>
          <seg type="desc">
            <date>1835</date>ein Attentat auf den französischen König versuchte; später zu
          </seg>
          <cit type="etymon">
            <form>
              <orth>Fiesco</orth>
            </form>
          </cit>
          <seg type="desc">umgedeutet, nach Giovanni Luigi de Fieschi (
            <date>1524/1525-1547</date>), der eine Verschwörung gegen das führende genuesische Adelsgeschlecht versuchte und durch Friedrich Schillers „Die Verschwörung des Fiesco zu Genua“ (
          </seg>
          <seg type="desc">
            <date>1783</date>) bekannt wurde
          </seg>
        </etym>
        <sense>
          <sense>
            <sense>
              <def type="umschreibung">Bezeichnung für ganz kurz geschnittene oder abgeschorene Haare</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.3">Waldv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.4">Weinv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">I lôs ma d' Hoaʳ fiésko schneiden</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.1s08">Zeiselmauer-Wolfpassing</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c5441"/>
            </sense>
            <sense>
              <def type="umschreibung">Bezeichnung für einen kurz geschnittenen oder rasierten Bart</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.3">Waldv.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">fiéskgo scheāin</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.3a15">Kautzen</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c5442"/>
            </sense>
          </sense>
        </sense>
      </entry>
