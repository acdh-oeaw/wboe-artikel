              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c44474"/>
            </sense>
          </sense>
          <sense>
            <sense>
              <def>schmeicheln; besänftigen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.1">ObKtn.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c44472"/>
            </sense>
            <sense>
              <def>ärgern; jemanden verärgern, verstimmen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1A.4">öSTir.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">daß braucht di gor ött zu gutschlⁿ</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p1A.4g01">Gsies</placeName>
                </usg>
              </cit>
              <cit type="example">
                <quote xml:lang="bar-AT">des gutšlt in</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p1A.4d02">Kiens</placeName>
                </usg>
              </cit>
              <cit type="example">
