              <usg>(Diminutiv)</usg>
              <def>(künstlerische) Darstellung eines schönen, ebenmäßig geformten menschlichen Körpers</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.2">Industriev.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">a Kobf wiara Figírl</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.2b20">Pfaffstätten</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c22744"/>
            </sense>
            <sense>
              <def>Gipsabguss einer Skulptur</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c22745"/>
            </sense>
          </sense>
          <sense>
            <sense>
