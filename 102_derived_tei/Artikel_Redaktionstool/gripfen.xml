                      <quote xml:lang="de">Backe</quote>
                    </cit>gripfa
                  </quote>
                  <usg type="geo">
                    <placeName type="gemeinde" ref="/geodata-wboe.xml#p1C.1f06">Lermoos</placeName>
                  </usg>
                </cit>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c35169"/>
              </sense>
              <sense>
                <def>kitzeln</def>
