                  <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.1">wNTir.</placeName>
                </usg>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c35165"/>
              </sense>
              <sense>
                <def>herumzupfen; wegzupfen (z. B. an einer Wunde)</def>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.1">wNTir.</placeName>
                </usg>
                <cit type="example">
                  <quote xml:lang="bar-AT">Tuᵃ nit oᵃlli drun gripfn, sinscht hoᵃlts Der gor num̄a zua!</quote>
                  <usg type="geo">
                    <placeName type="gemeinde" ref="/geodata-wboe.xml#p1C.1m02">Fendels</placeName>
                  </usg>
                </cit>
                <cit type="example">
                  <quote xml:lang="bar-AT">t haut gripfɒ</quote>
                  <usg type="geo">
                    <placeName type="gemeinde" ref="/geodata-wboe.xml#p1C.1m10">Serfaus</placeName>
                  </usg>
                </cit>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c35167"/>
              </sense>
              <sense>
                <def>in der Nase bohren</def>
