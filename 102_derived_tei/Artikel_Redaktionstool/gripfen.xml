        <sense>
          <sense>
            <sense>
              <sense>
                <def>(rasch, wiederholt) zugreifen; fassen</def>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.1">wNTir.</placeName>
