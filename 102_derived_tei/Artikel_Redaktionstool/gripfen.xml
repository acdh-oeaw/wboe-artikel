                  <placeName type="grossregion" ref="/geodata-wboe.xml#p1B.0">OTir.</placeName>
                </usg>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c35170"/>
              </sense>
            </sense>
          </sense>
          <sense>
