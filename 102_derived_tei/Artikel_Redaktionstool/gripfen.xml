                  <placeName type="grossregion" ref="/geodata-wboe.xml#p1A.1">wSTir.</placeName>
                </usg>
                <cit type="example">
                  <quote xml:lang="bar-AT">In der Nås gripfn</quote>
                  <usg type="geo">
                    <placeName type="gemeinde" ref="/geodata-wboe.xml#p1A.1a02">Mals</placeName>
                  </usg>
                </cit>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c35160"/>
              </sense>
              <sense>
                <def>zwicken; kratzen</def>
                <usg type="geo">
                  <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
                </usg>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p1B.0">OTir.</placeName>
                </usg>
                <usg type="geo">
                  <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
                </usg>
                <cit type="example">
                  <quote xml:lang="bar-AT">ear kån it recht Kitarschpīla, ear tuat lei aso gripfa</quote>
                  <usg type="geo">
                    <placeName type="gemeinde" ref="/geodata-wboe.xml#p1C.1m06">Nauders</placeName>
                  </usg>
