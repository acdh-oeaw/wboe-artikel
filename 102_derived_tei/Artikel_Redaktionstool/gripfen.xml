                <usg type="geo">
                  <placeName type="bundesland" ref="/geodata-wboe.xml#p2">Ktn.</placeName>
                </usg>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c35168"/>
              </sense>
            </sense>
            <sense>
              <sense>
                <def>herausrupfen; herausziehen; raufen</def>
