            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">Dǫ is weit umanond neamb sicha mit deina Fuchtlarei</quote>
              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p1C.3e06">Kufstein</placeName>
              </usg>
            </cit>
            <cit type="example">
