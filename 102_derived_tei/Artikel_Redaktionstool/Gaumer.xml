              <placeName type="grossregion" ref="/geodata-wboe.xml#p3.5">OStmk.</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c34037"/>
          </sense>
          <sense>
            <sense>
              <def>Beobachter; Zuseher</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.5">OStmk.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c34038"/>
            </sense>
            <sense>
              <def>ungeladener Hochzeitsgast</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.5">OStmk.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c34039"/>
            </sense>
          </sense>
          <sense>
            <def>Gusto; Lust</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p3.1">sbair.ObStmk.</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c34040"/>
          </sense>
        </sense>
        <re>
          <form subtype="compound" type="lemma">
            <orth>Mährengaumer</orth>
          </form>
