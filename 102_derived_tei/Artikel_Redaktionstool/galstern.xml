        </usg>
        <etym>
          <cit type="etymon"></cit>
          <seg type="desc">Ableitung von
            <xr type="lemma">
              <ref target="fackisch.xml">Galst</ref>
            </xr>
          </seg>
          <cit type="etymon">
            <def>Lärm; Geschrei; Getöse</def>
          </cit>
          <cit type="etymon">
            <lang>mhd.</lang>
            <form>
              <orth>galst</orth>
            </form>
            <def>Schrei</def>
          </cit>
          <seg type="desc">bzw.</seg>
          <cit type="etymon">
            <form>
              <orth>galster</orth>
            </form>
            <def>Zauber; (Zauber-)gesang</def>
          </cit>
          <seg type="desc">. Das Wort steht vermutlich in einem engen Zusammenhang mit
            <xr type="lemma">
              <ref>(Ge)alster</ref>
            </xr>
          </seg>
          <seg type="desc"></seg>
          <cit type="etymon">
            <def>mutwilliges Treiben (nach Art der Elstern)</def>
          </cit>
          <seg type="desc">und mit
            <xr type="lemma">
              <ref>alstern</ref>
            </xr>
          </seg>
          <seg type="desc"></seg>
          <cit type="etymon">
            <def>schnattern; plaudern; herumspringen</def>
          </cit>
          <seg type="desc">, wobei es vermutlich zu semantischen und lautlichen Vemischungen gekommen ist</seg>
          <ref type="bibl" target="#Lexer1872I">
            <lbl>vgl.</lbl>Lexer 1872 I
            <citedRange>730</citedRange>
          </ref>
          <ref type="bibl" target="#DWB1878IV">DWB 1878 IV
            <citedRange>1205, 3053</citedRange>
          </ref>
          <ref type="bibl" target="#WBÖ1970I">WBÖ 1970 I
            <citedRange>155f.</citedRange>
          </ref>
        </etym>
        <sense>
          <sense>
            <sense>
              <sense>
                <def>närrisch, kindisch und laut herumtollen, herumspringen, herumlaufen</def>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
                </usg>
                <usg type="geo">
                  <placeName type="bundesland" ref="/geodata-wboe.xml#p3">Stmk.</placeName>
                </usg>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p6.2">Industriev.</placeName>
                </usg>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c44909"/>
              </sense>
              <sense>
                <def>lärmend scherzen; vorwitzig sein</def>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p3.2">mbair.ObStmk.</placeName>
                </usg>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p3.3">WStmk.</placeName>
                </usg>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c44911"/>
              </sense>
              <sense>
                <def>schwätzen; plaudern; plauschen</def>
                <usg type="geo">
                  <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
                </usg>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p1B.0">OTir.</placeName>
                </usg>
                <usg type="geo">
                  <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
                </usg>
                <usg type="geo">
                  <placeName type="bundesland" ref="/geodata-wboe.xml#p2">Ktn.</placeName>
                </usg>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p3.2">mbair.ObStmk.</placeName>
                </usg>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
                </usg>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c44910"/>
              </sense>
            </sense>
            <sense>
              <sense>
                <def>stark husten</def>
                <usg type="geo">
                  <placeName type="bundesland" ref="/geodata-wboe.xml#p2">Ktn.</placeName>
                </usg>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p4.3">Pong.</placeName>
                </usg>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c44912"/>
              </sense>
              <sense>
                <def>unnatürlich und absichtlich laut weinen</def>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
                </usg>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c4491"/>
              </sense>
            </sense>
          </sense>
          <sense>
            <def>laut tönen, schallen</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p4.3">Pong.</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c44913"/>
          </sense>
        </sense>
