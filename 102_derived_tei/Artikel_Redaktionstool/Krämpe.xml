          <cit type="etymon">
            <form>
              <orth>grampen</orth>
            </form>
          </cit>
          <seg type="desc">aus</seg>
          <seg type="desc">
            <lang>mhd.</lang>
          </seg>
          <cit type="etymon">
            <form>
              <orth>grempeln</orth>
            </form>
            <form>
              <orth>grempen</orth>
            </form>
            <def>Kleinhandel treiben</def>
