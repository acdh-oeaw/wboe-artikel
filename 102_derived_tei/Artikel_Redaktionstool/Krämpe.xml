          <seg type="desc">; entlehnt aus der dialektalen Form</seg>
          <cit type="etymon">
            <form>
              <orth>cromprare</orth>
            </form>
          </cit>
          <seg type="desc">für</seg>
          <cit type="etymon">
            <lang>ital.</lang>
            <form>
              <orth>comprare</orth>
            </form>
            <def>kaufen</def>
          </cit>
          <ref type="bibl" subtype="online">
            <lbl>vgl.</lbl>Lexer online
            <xr type="lemma">
