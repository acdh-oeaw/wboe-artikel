              <date type="accessed">30.03.2023</date>
            </xr>
          </ref>
        </etym>
        <sense>
          <sense>
            <sense>
              <def>weibliche Person vom Land, die Lebensmittel oder Kleinwaren in der Stadt verkauft</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1B.0">OTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c33880"/>
            </sense>
            <sense>
              <def>weibliche Person, die im Auftrag einer anderen etwas überbringt; Botin</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1A.2">mSTir.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c33881"/>
            </sense>
          </sense>
        </sense>
        <re>
          <form subtype="compound" type="lemma">
            <orth>Bozenerkrämpe</orth>
