      <entry>
        <form type="lemma" xml:id="knerren">
          <orth>knerren</orth>
          <ptr type="collection" subtype="lemma" target="/collections-wboe.xml#c30388"/>
        </form>
        <gramGrp>
          <gram type="pos">Verb</gram>
          <iType>schwach</iType>
        </gramGrp>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p1B.0">OTir.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p2">Ktn.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p5">OÖ</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p6.2">Industriev.</placeName>
        </usg>
        <note type="kommentar">in Bearbeitung</note>
        <etym>
          <cit type="etymon"></cit>
          <seg type="desc">Ableitung von</seg>
          <cit type="etymon">
            <xr type="lemma">
              <ref target="knarren.xml">knarren</ref>
            </xr>
            <ref type="bibl" subtype="online">DWB online
              <xr type="lemma">
                <ref>knärren, knerren</ref>
                <date type="accessed">20.04.2023</date>
              </xr>
            </ref>
          </cit>
        </etym>
        <sense>
          <sense>
            <sense>
              <def>einen schnarrenden, knackenden, klanglosen Laut von sich geben</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.3">Traunv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">d' Geigna thoan knerrn</quote>
                <ref type="bibl" target="#Kaltenbrunner1878">Kaltenbrunner 1878
                  <citedRange>99</citedRange>
                </ref>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.3l02">Enns</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c31349"/>
            </sense>
            <sense>
              <def>ein hartes, reibendes Geräusch von sich geben; knirschen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c31357"/>
            </sense>
          </sense>
          <sense>
            <sense>
              <def>jemanden durch Stoßen oder Klagen zu etwas drängen oder antreiben</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1B.0">OTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.1">Innv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.4">Mühlv.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">m̱id lautə dnęəʳn is ə šąⁿ gąns tsąmkęmə</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.1c07">Mühlheim am Inn</placeName>
                </usg>
              </cit>
              <cit type="example">
                <quote xml:lang="bar-AT">si hǫd šąⁿ oėwėˡ gnęəd</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.1c07">Mühlheim am Inn</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c31343"/>
            </sense>
            <sense>
              <def>schimpfen; poltern; toben</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.1">Innv.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">dęə gnęəʳd widə wǫs tsąm!</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.1c07">Mühlheim am Inn</placeName>
                </usg>
              </cit>
              <cit type="example">
                <quote xml:lang="bar-AT">də wind gnęəʳd</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.1c07">Mühlheim am Inn</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c31348"/>
            </sense>
            <sense>
              <def>mürrisch reden; klagen; jammern</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p2">Ktn.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p5">OÖ</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.2">Hausrv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.2">Industriev.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c31350"/>
            </sense>
            <sense>
              <def>streiten; zanken</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p2">Ktn.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.1">Innv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c31346"/>
            </sense>
            <sense>
              <def>durcheinanderbringen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.1">Innv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c31345"/>
            </sense>
          </sense>
          <sense>
            <def>legen (von Eiern)</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">won d'Henn s Letzti Oar dneart</quote>
              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.1q06">Pyhra</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c31370"/>
          </sense>
        </sense>
        <re>
          <form subtype="compound" type="lemma">
            <orth>umknerren</orth>
          </form>
          <sense>
            <sense>
              <sense>
                <def>drängen; stoßen; unruhig sein</def>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p5.1">Innv.</placeName>
                </usg>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c31378"/>
              </sense>
              <sense>
                <def>herumtoben; lärmen (von Kindern)</def>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p5.1">Innv.</placeName>
                </usg>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c31379"/>
              </sense>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>zusammenknerren</orth>
          </form>
          <sense>
            <sense>
              <def>etwas durch angestrengte Arbeit vollenden</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.1">Innv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c31373"/>
            </sense>
          </sense>
        </re>
      </entry>
