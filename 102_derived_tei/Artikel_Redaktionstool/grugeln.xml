    <body>
      <entry>
        <form type="lemma" xml:id="grugeln">
          <orth>grugeln</orth>
          <ptr type="collection" subtype="lemma" target="/collections-wboe.xml#c32436 "/>
        </form>
        <gramGrp>
          <gram type="pos">Verb</gram>
          <iType>schwach</iType>
        </gramGrp>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p1B">OTir.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p4.2">Pinzg.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p5.1">Innv.</placeName>
        </usg>
        <note type="kommentar">folgt</note>
        <etym>
          <seg type="desc">wahrscheinlich Übertragung von lautmalerischem</seg>
          <cit type="etymon">
            <form>
              <orth>gru gru</orth>
            </form>
            <def>Ruf der Taube</def>
          </cit>
          <seg type="desc">;</seg>
          <seg type="desc">tritt auch in anderen, tendenziell im Westen des Sprachgebiets gesprochenen hochdeutschen Dialekten auf, vgl. z. B.:</seg>
          <cit type="etymon">
            <lang>pfälz.</lang>
            <form>
              <orth>gruksen</orth>
              <orth>grukseln</orth>
            </form>
            <def>gurren; knurren</def>
          </cit>
          <ref type="bibl" subtype="online">Pfälzisches Wörterbuch
            <xr type="lemma">
              <ref>grukseln; gruksen</ref>
              <date type="accessed">07.02.2023</date>
            </xr>
          </ref>
          <seg type="desc">,
            <lang>elsäss.</lang>
          </seg>
          <cit type="etymon">
            <form>
              <orth>kruxeⁿ</orth>
            </form>
            <def>klagen</def>
          </cit>
          <ref type="bibl" subtype="online">Wörterbuch der elsässischen Mundarten
            <xr type="lemma">
              <ref>kruxeⁿ</ref>
              <date type="accessed">07.02.2023</date>
            </xr>
          </ref>
          <seg type="desc">,
            <lang>schweiz.</lang>
          </seg>
          <cit type="etymon">
            <form>
              <orth>grochseⁿ</orth>
              <orth>gruchseⁿ</orth>
            </form>
            <def>ächzen, jammern, klagen, seufzen, stöhnen, schwer atmen, Schmerzenslaute ausstossen</def>
          </cit>
          <ref type="bibl" subtype="online">Schweizerisches Idiotikon
            <xr type="lemma">
              <ref>grochseⁿ, gruchseⁿ</ref>
              <date type="accessed">07.02.2023</date>
            </xr>
          </ref>
          <seg type="desc">;</seg>
          <seg type="desc">Im DWb werden ähnliche Bedeutungskombinationen auch für die Verben</seg>
          <cit type="etymon">
            <form>
              <orth>gurgeln</orth>
            </form>
          </cit>
          <seg type="desc">und</seg>
          <cit type="etymon">
            <form>
              <orth>gurren</orth>
            </form>
          </cit>
          <seg type="desc">angegeben, weshalb eine gegenseitige Beeinflussung nicht auszuschließen ist</seg>
          <seg type="desc">
            <ref type="bibl" target="#DWB1934IVIVI">
              <lbl>vgl.</lbl>DWB 1935 IV/I/IV
              <citedRange>Sp. 1152, Z. 38; Sp. 1165, Z. 39</citedRange>
            </ref>
          </seg>
          <seg type="desc">.</seg>
        </etym>
        <sense>
          <sense>
            <sense>
              <def>charakteristischen Taubenlaut von sich geben; gurren</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1B">OTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p4.2">Pinzg.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">ęntərn pǫχ entn, do grūgln di taubm, / iəts muəs i giən ummi, di fȫdərn
                  <cit type="translation">
                    <quote xml:lang="de">Federn</quote>
                  </cit>aukχlaubm; / di fȫdərn sain aukχlaup, štökχn oubmant main huət; / išt kχǫa frišər puə dō, deər mər sẹ ōərdərtuət?
                </quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p1C.2a29">Weer</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32442 "/>
              <sense></sense>
            </sense>
            <sense>
              <def>charakteristischen Katzenlaut von sich geben; schnurren</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p4.2">Pinzg.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32443 "/>
            </sense>
          </sense>
          <sense>
            <sense>
              <def>heiser atmen; röcheln</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1B">OTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.1">Innv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32444 "/>
            </sense>
            <sense>
              <def>leise murren, schimpfen, Unmut äußern</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1B">OTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32445 "/>
            </sense>
          </sense>
          <sense>
            <sense>
              <def>ein gurgelndes Geräusch von sich geben</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.1">wNTir.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32446 "/>
            </sense>
            <sense>
              <def>leise donnern</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.1">wNTir.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32447 "/>
            </sense>
          </sense>
        </sense>
      </entry>
