    <body>
      <entry>
        <form type="lemma" xml:id="golken">
          <orth>golken</orth>
        </form>
        <form type="lemma" xml:id="golkern">
          <orth>golkern</orth>
          <ptr type="collection" subtype="lemma" target="/collections-wboe.xml#c33771"/>
        </form>
        <gramGrp>
          <gram type="pos">Verb</gram>
          <iType>schwach</iType>
        </gramGrp>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.1">wNTir.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
        </usg>
        <etym>
          <seg type="desc">vermutlich eine Ableitung zu
            <lang>mhd.</lang>
          </seg>
          <cit type="etymon">
            <form>
              <orth>kolc</orth>
            </form>
            <def>Strudel; Abgrund</def>
          </cit>
          <seg type="desc">, eine urspüngliche Entlehnung aus romanischen Sprachen kann angenommen werden, vgl. dazu</seg>
          <cit type="etymon">
            <lang>ital.</lang>
            <form>
              <orth>gorgogliare</orth>
            </form>
            <def>gluckern; gurgeln</def>
          </cit>
          <ref type="bibl" target="#Lexer1872I">
            <lbl>vgl.</lbl>Lexer 1872 I
            <citedRange>1664</citedRange>
          </ref>
        </etym>
        <sense>
          <sense>
            <sense>
              <xr type="lemma">
                <lbl>nur</lbl>
                <ref>golken</ref>
              </xr>
              <def>etwas Flüssiges über den Gefäßrand kippen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.1">wNTir.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c44925"/>
            </sense>
            <sense>
              <xr type="lemma">
                <lbl>nur</lbl>
                <ref>golkern</ref>
              </xr>
              <def>rollen; kullern</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">'s Faßlₑ is ibrn Ro͜an åchₑ gᵃggolggrt</quote>
                <usg type="geo">
                  <placeName type="kleinregion" ref="/geodata-wboe.xml#p2.2d">obstGurkt.</placeName>
                </usg>
              </cit>
              <cit type="example">
                <quote xml:lang="bar-AT">s Oalₑ is mᵃʳ afn Podn åche gᵉggolggᵃʳt und nit amål brochn</quote>
                <usg type="geo">
                  <placeName type="kleinregion" ref="/geodata-wboe.xml#p2.3b">obGurkt.</placeName>
                </usg>
              </cit>
              <cit type="example">
                <quote xml:lang="bar-AT">a Fåß ggolggrn</quote>
                <usg type="geo">
                  <placeName type="kleinregion" ref="/geodata-wboe.xml#p2.3d">uGurkt.</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c44918"/>
            </sense>
          </sense>
        </sense>
        <re>
          <form subtype="compound" type="lemma">
            <orth>abgolkern</orth>
          </form>
          <sense>
            <sense>
              <def>abstürzen (von Weidevieh)</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c44922"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>abhingolkern</orth>
          </form>
          <sense>
            <sense>
              <def>hinunterrollen; hinunterkullern</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.3">MKtn.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">s Biᵃblₑ is ibᵃʳn Rₒan åbₑ gₐggolggᵃʳt</quote>
                <usg type="geo">
                  <placeName type="kleinregion" ref="/geodata-wboe.xml#p2.3c">Krappfd.</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c44923"/>
            </sense>
          </sense>
        </re>
        <re type="redewendung">
          <form subtype="MWE" type="lemma">
            <orth>das Ringlein golkern</orth>
          </form>
          <sense>
            <def type="umschreibung">Spiel</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p2.3">MKtn.</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">s Ringlₑ ggolggrn</quote>
              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p2.3b05">Straßburg</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c44919"/>
          </sense>
        </re>
