              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c7303"/>
            </sense>
          </sense>
        </sense>
