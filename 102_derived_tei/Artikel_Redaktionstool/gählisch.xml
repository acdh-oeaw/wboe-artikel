            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">gaalisch weard sie köima</quote>
              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p1C.1n05">Landeck</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c25039"/>
          </sense>
        </sense>
