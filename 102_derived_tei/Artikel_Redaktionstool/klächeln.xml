                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.2e06">Schwarzau im Gebirge</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c43053"/>
            </sense>
            <sense>
              <def>herumschlendern; müßig herumstreifen; gemächlich wandern; faulenzen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.3">öNTir.</placeName>
