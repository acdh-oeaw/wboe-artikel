        <etym>
          <cit type="etymon">
            <lang>frnhd.</lang>
            <form>
              <orth>klacheln</orth>
            </form>
            <form>
              <orth>klächeln</orth>
            </form>
            <def>baumeln; plump einhergehen; faul herumschlendern; bummeln; tönen (wenn der Ton auf Holz erzeugt wird); klopfen</def>
          </cit>
          <ref type="bibl" subtype="online">
            <lbl>vgl.</lbl>DWB online
            <xr type="lemma">
              <ref>klacheln, klächeln</ref>
              <date type="accessed">25.04.2024</date>
            </xr>
          </ref>
          <seg type="desc">; Ableitung von</seg>
          <cit type="etymon">
            <xr type="lemma">
              <ref target="Klächel.xml#Klaechel">Klächel</ref>
            </xr>
          </cit>
        </etym>
        <sense>
          <sense>
            <sense>
              <def>baumeln; sich schwebend hin- und herbewegen</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1B.0">OTir.</placeName>
