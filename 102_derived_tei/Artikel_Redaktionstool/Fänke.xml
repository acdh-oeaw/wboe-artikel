          <usg type="geo">
            <placeName type="gemeinde" ref="/geodata-wboe.xml#p1C.1n01">Imst</placeName>
          </usg>
          <ptr type="collection" subtype="lautung" target="/collections-wboe.xml#c5644"/>
        </form>
        <etym>
