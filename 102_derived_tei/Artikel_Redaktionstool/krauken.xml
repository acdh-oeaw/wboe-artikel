                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p4.5">Flachg.</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c37535"/>
