        </usg>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
        </usg>
        <note type="kommentar"></note>
        <etym>
          <seg type="desc">Ableitung von</seg>
          <cit type="etymon">
            <xr type="lemma">
              <ref target="Kummer.xml#Kummer">Kummer</ref>
            </xr>
          </cit>
        </etym>
        <sense>
          <sense>
            <def>Verantwortung für etwas oder jemanden tragen (müssen); etwas oder jemanden umsorgen (müssen)</def>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p1B">OTir.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p4.3">Pong.</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">kimmɛrt ihn</quote>
              <ref type="bibl" target="#TirolerWb">Tiroler Wb.
                <citedRange>362</citedRange>
              </ref>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40636 "/>
          </sense>
          <sense>
            <sense>
              <xr type="lemma">
                <lbl>nur</lbl>
                <ref>sich kümmern</ref>
              </xr>
              <usg>(nur reflexiv)</usg>
              <def>jemanden oder sich selbst (gedanklich) mit etwas belasten; jemanden oder etwas (gedanklich) berücksichtigen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.2">mbair.ObStmk.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.1">Innv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.3">Traunv.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">ęɒ khimɒd sĕ um nęɒmt</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.1c07">Mühlheim am Inn</placeName>
                </usg>
              </cit>
              <cit type="example">
                <quote xml:lang="bar-AT">ę̄ khiməʳt sö um niks</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.3b02">Gmunden</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40637 "/>
            </sense>
            <sense>
              <def>jemanden durch Klagen und dringende bis belästigende Bitten plagen/quälen</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p2">Ktn.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p3">Stmk.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40639 "/>
            </sense>
          </sense>
          <sense>
            <def>dahinvegetieren; klein und schwach sein</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p4.3">Pong.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p5">OÖ</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40638 "/>
          </sense>
        </sense>
        <re>
          <form subtype="compound" type="lemma">
            <orth>bekümmern</orth>
          </form>
          <sense>
            <sense>
              <def>jemanden oder sich selbst gedanklich mit etwas belasten</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p4.3">Pong.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.3">Traunv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.4">Weinv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40623 "/>
            </sense>
            <sense>
              <xr type="lemma">
                <lbl>nur</lbl>
                <ref>sich bekümmern</ref>
              </xr>
              <usg>(nur reflexiv)</usg>
              <def>sich um jemanden oder etwas bemühen; versuchen etwas zu erreichen/bekommen</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">Bekima di a bissl um a Maᵈl</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.5i02">Lichtental</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40624 "/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>verkümmern</orth>
          </form>
          <sense>
            <sense>
              <def>an Substanz verlieren; klein und schwach werden</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.2">Industriev.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40626 "/>
            </sense>
            <sense>
              <def>etwas verpfänden; etwas verkaufen</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p3">Stmk.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40625 "/>
            </sense>
          </sense>
        </re>
        <re type="redewendung">
          <form subtype="MWE" type="lemma">
            <orth>es kümmert ihn/sie nichts</orth>
          </form>
          <sense>
            <def>es geht ihn/sie nichts an; es ist nicht seine/ihre Angelegenheit</def>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p1B">OTir.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">es kimmɛrt ihn nichts</quote>
              <ref type="bibl" target="#TirolerWb">Tiroler Wb.
                <citedRange>362</citedRange>
              </ref>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1B">OTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40628 "/>
          </sense>
        </re>
        <re type="redewendung">
          <form subtype="MWE" type="lemma">
            <orth>Kümmere dich nicht!</orth>
          </form>
          <sense>
            <def>Lass die Leute reden! Nimm dir nicht zu Herzen, was andere Leute sagen!</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p5.4">Mühlv.</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">Kimar dė nėt!</quote>
              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.4b13">Ulrichsberg</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40627 "/>
          </sense>
        </re>
        <re type="redewendung">
          <form subtype="MWE" type="lemma">
            <orth>nicht kümmern</orth>
          </form>
          <sense>
            <def>etwas vernachlässigen</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p6.4">Weinv.</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">nid kiman</quote>
              <usg type="geo">
                <placeName type="kleinregion" ref="/geodata-wboe.xml#p6.4l">westl.mMarchfd.</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40630 "/>
          </sense>
        </re>
        <re type="redewendung">
          <form subtype="MWE" type="lemma">
            <orth>ohne sich zu kümmern</orth>
          </form>
          <sense>
            <def>unbekümmert; naiv; sorgenfrei</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p6.4">Weinv.</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">ohni sih zkiman</quote>
              <usg type="geo">
                <placeName type="kleinregion" ref="/geodata-wboe.xml#p6.4l">westl.mMarchfd.</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40631 "/>
          </sense>
        </re>
