          </usg>
          <ptr type="collection" subtype="lautung" target="/collections-wboe.xml#c19458"/>
        </form>
        <etym>
          <cit type="etymon">
            <lang>mhd.</lang>
            <form>
              <orth>vuozen</orth>
            </form>
            <def>treten; sich stützen; stoßen</def>
          </cit>
          <ref type="bibl" target="#Lexer1878III">Lexer 1878 III
            <citedRange>580</citedRange>
          </ref>
          <seg type="desc">; Ableitung von</seg>
          <cit type="etymon">
            <xr type="lemma">
              <ref target="Fuß.xml#Fuss">Fuß</ref>
            </xr>
          </cit>
        </etym>
        <sense>
          <sense>
            <def>eilen; sich beeilen; schnell gehen; laufen</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p3">Stmk.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p5.4">Mühlv.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p6">NÖ</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">Du, fuaß di!</quote>
              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.4h06">Pabneukirchen</placeName>
              </usg>
            </cit>
            <cit type="example">
              <quote xml:lang="bar-AT">fuaß di a weng</quote>
              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.1k05">Kilb</placeName>
              </usg>
            </cit>
            <cit type="example">
              <quote xml:lang="bar-AT">Ea soiˡ si fuaßn!</quote>
              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.1n02">Dunkelsteinerwald</placeName>
              </usg>
            </cit>
            <cit type="example">
              <quote xml:lang="bar-AT">i muaß mi fuaßen</quote>
              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.2h28">Warth</placeName>
              </usg>
            </cit>
            <cit type="example">
              <quote xml:lang="bar-AT">fuis di</quote>
              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.3a15">Kautzen</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c19461"/>
          </sense>
          <sense>
            <xr type="variant" subtype="compound">
              <lbl>nur</lbl>
              <ref>gefußt</ref>
            </xr>
            <usg>(adjektivisches Partizip)</usg>
            <def>mit Füßen versehen</def>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p3">Stmk.</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">3 gefuoste Pfannen</quote>
              <ref type="bibl" target="#SteirWb1903">Steir.Wb. 1903
                <citedRange>132</citedRange>
              </ref>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p3">Stmk.</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c19463"/>
          </sense>
        </sense>
