                </usg>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c39987"/>
              </sense>
            </sense>
            <sense>
              <def>Kübel aus Holz für das Schweinefutter</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.1">ObKtn.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c39986"/>
            </sense>
          </sense>
        </sense>
