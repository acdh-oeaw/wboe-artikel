          <ref type="bibl" subtype="online">
            <lbl>vgl.</lbl>Pons online
            <xr type="lemma">
              <ref>convoyer</ref>
              <date type="accessed">29.06.2023</date>
            </xr>
          </ref>
          <seg type="desc">; vergleiche auch im Schweizerdeutschen</seg>
          <cit type="etymon">
            <form>
              <orth>kumfójeⁿ</orth>
            </form>
            <def>drängen; nötigen; weg-, verschleppen</def>
          </cit>
          <ref type="bibl" target="#SchweizerischesId1895III">
            <lbl>vgl.</lbl>Schweizerisches Id. 1895 III
            <citedRange>304</citedRange>
          </ref>
        </etym>
        <sense>
