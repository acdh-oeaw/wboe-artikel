                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c38664"/>
              </sense>
              <sense>
                <def>strafen</def>
                <usg type="geo">
                  <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
                </usg>
