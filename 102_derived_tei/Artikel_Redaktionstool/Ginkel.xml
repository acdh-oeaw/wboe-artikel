              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p5">OÖ</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p6">NÖ</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">dę Buə, dá̊s īs a Šbriŋgiŋkəʳl</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.3b02">Gmunden</placeName>
                </usg>
              </cit>
              <cit type="example">
                <quote xml:lang="bar-AT">der is a rechta (rechter) Springinkal</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.4h06">Pabneukirchen</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32726"/>
            </sense>
            <sense>
              <def>Springkraut; Impatiens noli-tangere</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p5">OÖ</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32727"/>
            </sense>
          </sense>
