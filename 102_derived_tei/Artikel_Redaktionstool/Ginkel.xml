        <note type="kommentar">in Arbeit</note>
        <etym>
          <seg type="desc">Die genaue Herkunft ist unklar. Vermutlich besteht ein Zusammenhang mit dem in verschiedenen deutschen Mundarten belegten Verb</seg>
          <cit type="etymon">
            <form>
              <orth>gankeln</orth>
            </form>
            <form>
              <orth>kankeln</orth>
            </form>
            <def>baumeln; wackeln; schaukeln</def>
          </cit>
          <seg type="desc">sowie mit</seg>
          <cit type="etymon">
            <xr type="lemma">
              <ref target="Gänker.xml#Gaenker">Gänker</ref>
            </xr>
          </cit>
          <seg type="desc">(bzw. dem Diminutiv</seg>
          <xr type="lemma" subtype="diminutive">
            <ref target="Gänker.xml#Gaenkerl">Gänkerl</ref>
          </xr>
          <seg type="desc">)</seg>
          <seg type="desc">, das u. a. die Bedeutungen</seg>
          <cit type="etymon">
            <def>Teufel; Satan</def>
          </cit>
          <seg type="desc">sowie</seg>
          <cit type="etymon">
            <def>leichtsinniger Mensch; Narr; Spinner</def>
          </cit>
          <seg type="desc">umfasst.</seg>
          <cit type="etymon">
            <ref type="bibl" target="#DWB1878IV/I/1">
              <lbl>vgl.</lbl>DWB 1878 IV/I/1
              <citedRange>1254</citedRange>
            </ref>
          </cit>
          <seg type="desc">Bezüglich der Hebung des Stammvokals zu
            <c>i</c>kann eine lautliche Assimilation mit dem Bestimmungswort in dem weit verbreiteten Kompositum
            <xr type="variant" subtype="compound">
              <ref>Springginkel</ref>
            </xr>angenommen werden; vgl. auch
          </seg>
          <cit type="etymon">
            <xr type="lemma">
              <ref target="Glänkel.xml#Glaenkel">Glänkel</ref>
            </xr>
          </cit>
          <seg type="desc">und</seg>
          <cit type="etymon">
            <xr type="lemma">
              <ref target="glänkeln.xml#glaenkeln">glänkeln</ref>
            </xr>
          </cit>
        </etym>
        <sense>
          <sense>
            <lbl>nur</lbl>
            <xr type="lemma" subtype="diminutive">
              <ref target="fachen.xml#Ginkerl">Ginkerlein</ref>
            </xr>
            <usg>(Diminutiv, Plural)</usg>
            <def type="umschreibung">bestimmte Art der weiblichen Haartracht</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.2">mNTir.</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32214"/>
          </sense>
          <sense>
            <def>Eisenstift mit Ring zum Holzziehen</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.1">wNTir.</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32212"/>
          </sense>
        </sense>
