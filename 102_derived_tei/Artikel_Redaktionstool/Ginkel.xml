                </usg>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32725"/>
              </sense>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>Springginkel</orth>
          </form>
          <sense>
            <sense>
              <def>lebhafter, fröhlicher, unbekümmerter junger Mensch; Springinsfeld</def>
