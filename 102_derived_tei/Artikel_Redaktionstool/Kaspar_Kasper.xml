    <body>
      <entry>
        <form type="lemma" xml:id="Kaspar">
          <orth>Kaspar</orth>
        </form>
        <form type="lemma" xml:id="Kasper">
          <orth>Kasper</orth>
          <ptr type="collection" subtype="lemma"/>
        </form>
        <gramGrp>
          <gram type="pos">Substantiv</gram>
          <gram type="gender">Maskulinum</gram>
        </gramGrp>
        <gramGrp>
          <gram type="pos">vor allem Name</gram>
        </gramGrp>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
        </usg>
