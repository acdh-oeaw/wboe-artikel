        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
        </usg>
        <etym>
          <seg type="desc">
            <lang>mhd.</lang>
          </seg>
          <cit type="etymon">
            <form>
              <orth>Kaspar</orth>
            </form>
          </cit>
          <seg type="desc">, Eigenname von einem der Heiligen Drei Könige
            <ref type="bibl" target="#BMZ1854I">
              <lbl>vgl.</lbl>BMZ 1854 I
              <citedRange>792a</citedRange>
            </ref>
          </seg>
          <seg type="desc">, auf diese Weise hat sich der Name im Mittelalter im deutschen Sprachgebiet verbreitet;</seg>
          <seg type="desc">ursprünglich entlehnt aus
            <lang>pers.</lang>
          </seg>
          <seg type="desc"></seg>
          <cit type="etymon">
            <form>
              <orth>kandschwar</orth>
            </form>
            <def>Schatzmeister</def>
          </cit>
          <ref type="bibl" target="#Seibicke1998II">
            <lbl>vgl.</lbl>Seibicke 1998 II
            <citedRange>286</citedRange>
          </ref>
        </etym>
        <sense>
          <sense>
            <sense>
              <def type="umschreibung">Ruf- bzw. Vorname (auch Heiligenname)</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1B">OTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p2">Ktn.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.1">sbair.ObStmk.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.2">mbair.ObStmk.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.3">WStmk.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p4">Sbg.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p5">OÖ</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p6">NÖ</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c"/>
            </sense>
            <sense>
              <def>Hanswurst im Prater</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung"/>
            </sense>
            <sense>
              <def>dummer, einfältiger oder ungeschickter, komischer Mensch, der sich lächerlich macht bzw. den man nicht ernst nimmt; Spaßvogel</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1A.2">mSTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.1">wNTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p2">Ktn.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.2">mbair.ObStmk.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p5">OÖ</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p6">NÖ</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">Der Koᵃsper hoᵃt si 's Gwoᵃnd verkeᵃhrt unglögt</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p1C.1m02">Fendels</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung"/>
            </sense>
            <sense>
              <def type="umschreibung">Schimpfwort</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.3">Waldv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.4">Weinv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung"/>
            </sense>
          </sense>
          <sense>
            <def type="umschreibung">Hausname</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c"/>
          </sense>
        </sense>
        <re type="redewendung">
          <form subtype="MWE" type="lemma">
            <orth>Du Kaspar Thoringer</orth>
          </form>
          <sense>
            <def>Du ungeschickter Mensch</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">Du Kaspar Thoringer</quote>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
            </cit>
          </sense>
          <ptr type="collection" subtype="MWE" target="/collections-wboe.xml#c"/>
        </re>
        <re type="redewendung">
          <form subtype="MWE" type="lemma">
            <orth>ein rechter Kaspar/Kasper sein</orth>
          </form>
          <sense>
            <sense>
              <def>ein ungeschickter, unbesonnener Mensch sein</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.1">Innv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.3">Waldv.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">bist ə ręchtə Khǫšpə</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.1c07">Mühlheim am Inn</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung"/>
            </sense>
            <sense>
              <def>ein Schalk, Spaßvogel, dummer Kerl sein</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.1">Innv.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">bist ə ręchtə Khǫšpə!</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.1c07">Mühlheim am Inn</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung"/>
            </sense>
          </sense>
        </re>
