          <ref type="bibl" target="#BMZ1854I">
            <lbl>vgl.</lbl>BMZ 1854 I
            <citedRange>859a</citedRange>
          </ref>
        </etym>
        <sense>
          <sense>
            <sense>
              <def>Mädchen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.4">Weinv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung"/>
            </sense>
            <sense>
              <def>große, starke (teilweise auch rüstige) Frau bzw. ebensolches Mädchen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p4.2">Pinzg.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.2">Industriev.</placeName>
