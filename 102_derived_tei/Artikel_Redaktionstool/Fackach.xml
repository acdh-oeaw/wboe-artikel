          <usg type="geo">
            <placeName type="gemeinde" ref="/geodata-wboe.xml#p2.3c01">Althofen</placeName>
          </usg>
        </form>
        <form type="dialect">
          <pron xml:lang="bar-AT">fåckach</pron>
          <usg type="geo">
            <placeName type="gemeinde" ref="/geodata-wboe.xml#p3.1a05">Stadl-Predlitz</placeName>
          </usg>
        </form>
        <form type="dialect">
