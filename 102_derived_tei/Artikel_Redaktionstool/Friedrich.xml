            <orth>Arsch und Friedrich</orth>
          </form>
          <sense>
            <def>gleichgültig; keine Rolle spielend; unwesentlich</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
