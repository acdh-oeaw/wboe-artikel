          <sense>
            <def>„Schweinerei treiben“</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p1A.1">wSTir.</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">Das erstemal ist geheiratet, das zweitemal geaht ohn, das drittemål ist lei gfåckt</quote>
              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p1A.1a02">Mals</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c5478"/>
          </sense>
          <sense>
