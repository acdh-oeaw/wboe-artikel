  <text>
    <body>
      <entry>
        <form type="lemma" xml:id="Greie">
          <orth>Greie</orth>
          <ptr type="collection" subtype="lemma" target="/collections-wboe.xml#c32539 "/>
        </form>
        <gramGrp>
          <gram type="pos">Substantiv</gram>
          <gram type="gender">Femininum</gram>
          <gram type="gender">Maskulinum</gram>
        </gramGrp>
        <form type="variant" subtype="diminutive" xml:id="Greiel">
          <orth>Greiel</orth>
          <gramGrp>
            <gram type="gender">Neutrum</gram>
          </gramGrp>
        </form>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p1B">OTir.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p2">Ktn.</placeName>
        </usg>
        <note type="kommentar"></note>
        <etym>
          <seg type="desc">unklar, eventuell entlehnt aus</seg>
          <cit type="etymon">
            <lang>mlat. oder galloromanisch</lang>
            <form>
              <orth>carruca</orth>
            </form>
            <def>(meist zweirädriges) Fahrzeug</def>
          </cit>
          <seg type="desc">oder der
            <lang>italienischen</lang>
          </seg>
          <seg type="desc">Weiterentwicklung</seg>
          <cit type="etymon">
            <form>
              <orth>carega</orth>
            </form>
          </cit>
          <ref type="bibl" subtype="online">
            <lbl>vgl.</lbl>Mittellateinisches Wörterbuch
            <xr type="lemma">
              <ref>carruca</ref>
              <date type="accessed">17.02.2023</date>
            </xr>
          </ref>
          <ref type="bibl" target="#Schneider1963">Schneider 1963
            <citedRange>543</citedRange>
          </ref>
          <ref type="bibl" target="#Prosch1921">Prosch 1912
            <citedRange>46</citedRange>
          </ref>
        </etym>
        <sense>
          <sense>
            <def>zweirädriger Karren oder Wagen (besonders für Heu oder Dünger)</def>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p1B">OTir.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p2.1">ObKtn.</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">In Stuls
                <cit type="translation">
                  <quote xml:lang="de">Ortschaft in Südtirol</quote>
                </cit>kann das Heu nur selten mit Graien eingeführt werden
              </quote>
              <ref type="bibl" target="#Hubatschek1948">Hubatschek 1948
                <citedRange>90</citedRange>
              </ref>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32542 "/>
          </sense>
          <sense>
            <sense>
              <def>Bretter- oder Balkenrahmen auf einem Wagen für schwere Lasten</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.1">wNTir.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32545 "/>
            </sense>
            <sense>
              <def>geflochtener Wagenkorb für Laub- oder Holzkohletransporte</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.2">mNTir.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32547 "/>
            </sense>
          </sense>
          <sense>
            <def>Vorrichtung zum Misttransport auf Steilhängen</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p1A.3">mSTir.</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">Greidn zuebetun</quote>
              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p1A.3b01">Sarntal</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32548 "/>
          </sense>
        </sense>
        <re>
          <form subtype="compound" type="lemma">
            <orth>Heugreie</orth>
          </form>
          <sense>
            <sense>
              <def>niederer Heuwagen; Heugitter</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1B">OTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32551 "/>
            </sense>
          </sense>
        </re>
      </entry>
