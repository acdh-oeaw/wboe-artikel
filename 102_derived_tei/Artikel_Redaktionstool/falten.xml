        <etym>
          <seg type="desc">Das Verb</seg>
          <cit type="etymon">
            <form>
              <orth>falten</orth>
            </form>
          </cit>
          <seg type="desc">geht auf ursprünglich starke (reduplizierende) Formen
            <lang>ahd.</lang>
