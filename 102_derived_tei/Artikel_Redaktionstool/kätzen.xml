    <body>
      <entry>
        <form type="lemma" xml:id="kaetzen">
          <orth>kätzen</orth>
          <ptr type="collection" subtype="lemma" target="/collections-wboe.xml#c38080"/>
        </form>
        <gramGrp>
          <gram type="pos">Verb</gram>
          <iType>schwach</iType>
        </gramGrp>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p1B.0">OTir.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p6.3">Waldv.</placeName>
        </usg>
        <note type="kommentar"></note>
        <etym>
          <seg type="desc">Die Herkunft ist teilweise unklar: Von</seg>
          <cit type="etymon">
            <xr type="lemma">
              <ref target="Katze.xml">Katze</ref>
            </xr>
          </cit>
          <seg type="desc">scheint vor allem die Bedeutung 'sich zerstreiten' abgeleitet zu sein
            <ref type="bibl" target="#">
              <lbl>vgl.</lbl>Wortbildung: zerkätzen
            </ref>
          </seg>
          <ref type="bibl" target="#DWB1873V">DWB 1873 V
            <citedRange>291</citedRange>
          </ref>
          <seg type="desc">. Die Bedeutung 'fangen (spielen)' dürfte auf das Spiel</seg>
          <cit type="etymon">
            <form>
              <orth>Katzball</orth>
            </form>
            <def>Fangball</def>
          </cit>
          <seg type="desc">, von</seg>
          <cit type="etymon">
            <lang>niederländ.</lang>
            <form>
              <orth>kaats, kaatze</orth>
            </form>
            <def>Ort im Ballhaus, wo der Ball zuerst aufspringt</def>
          </cit>
          <seg type="desc">,</seg>
          <cit type="etymon">
            <lang>franz.</lang>
            <form>
              <orth>chasse</orth>
            </form>
          </cit>
          <seg type="desc">, zurückzuführen sein</seg>
          <ref type="bibl" target="#DWB1873V">
            <lbl>vgl.</lbl>DWB 1873 V
            <citedRange>279</citedRange>
          </ref>
          <seg type="desc">.</seg>
        </etym>
        <sense>
          <sense>
            <sense>
              <def>etwas durch einen schnellen Ruck rasch auf die Seite werfen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1B.0">OTir.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40238"/>
            </sense>
            <sense>
              <def>etwas mit dem Finger oder der Nadelspitze wegschnellen (lassen)</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1B.0">OTir.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40310"/>
            </sense>
          </sense>
          <sense>
            <sense>
              <def>fangen; rasch ergreifen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.3">öNTir.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40309"/>
            </sense>
            <sense>
              <def>fangen spielen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.3">öNTir.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40308"/>
            </sense>
          </sense>
          <sense>
            <def>graben; scharren</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.2">mNTir.</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40305"/>
          </sense>
        </sense>
        <re>
          <form subtype="compound" type="lemma">
            <orth>auskätzen</orth>
          </form>
          <sense>
            <sense>
              <def>ausjagen; austreiben</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.3">Waldv.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">khāds āūs</quote>
                <def>interjektionale Phrase, mit der man Katzen etc. ausjagt</def>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.1o01">Herzogenburg</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40250"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>einkätzen</orth>
          </form>
          <sense>
            <sense>
              <def>eingraben; verscharren</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.2">mNTir.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40253 "/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>zerkätzen</orth>
          </form>
          <sense>
            <sense>
              <def>sich zerstreiten</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1B.0">OTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">Dia håmb si mídanånd z'kåtzt</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p1C.3i03">Kitzbühel</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40243"/>
            </sense>
          </sense>
        </re>
      </entry>
