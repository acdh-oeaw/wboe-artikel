                <placeName type="bundesland" ref="/geodata-wboe.xml#p5">OÖ</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c28393"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>Wassergieße</orth>
          </form>
          <sense>
            <sense>
              <def>heftiger Regen; Wolkenbruch; Unwetter mit viel Regen</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p3">Stmk.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.3">Traunv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c28396"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>Zinngieße</orth>
          </form>
          <sense>
            <sense>
              <def>Glocke mit hellem Ton</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p4.2">Pinzg.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c28391"/>
            </sense>
          </sense>
        </re>
      </entry>
