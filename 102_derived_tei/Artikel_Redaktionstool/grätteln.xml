        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p2.1">ObKtn.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p4.2">Pinzg.</placeName>
        </usg>
        <note type="kommentar"></note>
        <etym>
          <seg type="desc">Ableitung von</seg>
          <cit type="etymon">
            <xr type="lemma">
              <ref target="Gratte.xml">Gratte</ref>
            </xr>
          </cit>
        </etym>
        <sense>
          <sense>
            <sense>
              <def>mit einem (kleinen schlechten) Karren fahren</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1A.2">mSTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1B.0">OTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40085"/>
            </sense>
            <sense>
              <def>etwas (meist Erde oder Mist) mit einem Karren transportieren</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1B.0">OTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p4.2">Pinzg.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c39984"/>
            </sense>
            <sense>
              <def>das Seil an den Erdkarren anhängen (meist von Kindern verrichtet)</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.1">ObKtn.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40086 "/>
            </sense>
          </sense>
          <sense>
            <def>mühsam arbeiten; allerlei kleine Arbeiten verrichten</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p1A.2">mSTir.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.3">öNTir.</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40087"/>
          </sense>
          <sense>
            <def>Konkurs machen</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p1A.4">öSTir.</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">dēn grattlt s nou</quote>
              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p1A.4c01">Lüsen</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40088"/>
          </sense>
        </sense>
        <re>
          <form subtype="compound" type="lemma">
            <orth>aufgrätteln</orth>
          </form>
          <sense>
            <sense>
              <def>Erde oder Mist an einen höher gelegenen Ort transportieren (meist mit einem Karren oder einem Flaschenzug)</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1A.2">mSTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p4.2">Pinzg.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40075"/>
            </sense>
            <sense>
              <def>jemanden zugrunde richten</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">î pin gąnz aug[e]grattlt
                  <cit type="translation">
                    <quote xml:lang="de">ich bin am Ende meiner Kräfte</quote>
                  </cit>
                </quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p1A.3i02">Kastelruth</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40079"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>aufhingrätteln</orth>
          </form>
          <sense>
            <sense>
              <def>Erde oder Mist auf steile Äcker hinaufführen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.1">wNTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40076"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>umhergrätteln</orth>
          </form>
          <sense>
            <sense>
              <def>niederkommen; gebären</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1B">OTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40077"/>
            </sense>
          </sense>
        </re>
      </entry>
