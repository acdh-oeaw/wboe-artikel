    <body>
      <entry>
        <form type="lemma" xml:id="Grefte">
          <orth>Grefte</orth>
          <ptr type="collection" subtype="lemma" target="/collections-wboe.xml#c32637 "/>
        </form>
        <gramGrp>
          <gram type="pos">Substantiv</gram>
          <gram type="gender">Femininum</gram>
        </gramGrp>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p3">Stmk.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p6">NÖ</placeName>
        </usg>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
        </usg>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p7">Bgl.</placeName>
        </usg>
        <note type="kommentar"></note>
        <etym>
          <seg type="desc">zu</seg>
          <cit type="etymon">
            <lang>mhd.</lang>
            <form>
              <orth>greften</orth>
            </form>
            <def>graben</def>
          </cit>
          <ref type="bibl" target="#Lexer1872I">
            <lbl>vgl.</lbl>Lexer 1872 I
            <citedRange>Sp. 1076</citedRange>
          </ref>
          <seg type="desc">, das schon im Spätmittelalter in Ostösterreich die Bedeutung</seg>
          <cit type="etymon">
            <def>Gräben zum Rebensetzen herstellen; ein Feld in einen Weinberg verwandeln</def>
          </cit>
          <seg type="desc">ausdrückt
            <ref type="bibl" target="#DRWIV">
              <lbl>vgl.</lbl>DRW IV
              <citedRange>Sp. 1082–1083</citedRange>
            </ref>
          </seg>
        </etym>
        <sense>
          <sense>
            <sense>
              <def>Furche, in die beim Anlegen eines Weingartens die Weinreben gesetzt werden</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.4">Weinv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p7.1">NBgl.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32638 "/>
            </sense>
            <sense>
              <def>Weinstockreihen; Rebstockzeilen</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p6">NÖ</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32639 "/>
            </sense>
            <sense>
              <def>Raum, Durchgang zwischen zwei Weinstockreihen im Weingarten</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.3">Waldv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p7.1">NBgl.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32640 "/>
            </sense>
            <sense>
              <def>Weinstock</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p7">Bgl.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32641 "/>
            </sense>
            <sense>
              <def>Weinbergried; (junger) Weingarten</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p7.1">NBgl.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32642 "/>
            </sense>
          </sense>
          <sense>
            <sense>
              <def>Ackerfurche für Feldfrüchte im Allgemeinen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.3">Waldv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32644 "/>
            </sense>
            <sense>
              <def>Bodenanbau; Form des Fruchtanbaus in Feldern und Weingärten</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p3">Stmk.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32645 "/>
            </sense>
            <sense>
              <def>neu zu bearbeitendes Feld</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p3">Stmk.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32647 "/>
            </sense>
          </sense>
          <sense>
            <def>Graben; Furche</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p6.2">Industriev.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p7.1">NBgl.</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32648 "/>
          </sense>
          <sense>
            <def>Zwischenraum in zweizeiligen Steingärten, in dem gearbeitet wird</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p6.4">Weinv.</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32650 "/>
          </sense>
        </sense>
        <re>
          <form subtype="compound" type="lemma">
            <orth>Ortgrefte</orth>
          </form>
          <sense>
            <sense>
              <def>Weinstock, der den Greftenweingarten am vorderen und hinteren Ende begrenzt</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p7">Bgl.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32653 "/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>Paltengrefte</orth>
          </form>
          <sense>
            <sense>
              <def>erste und letzte Querfurche eines Weingartens, in die Weinreben gesetzt werden</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32652 "/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>Weingrefte</orth>
          </form>
          <sense>
            <sense>
              <def>Weingartenanlage</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p3">Stmk.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c32656 "/>
            </sense>
          </sense>
        </re>
      </entry>
