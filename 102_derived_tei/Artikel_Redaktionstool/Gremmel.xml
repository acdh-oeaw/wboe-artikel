        <form type="lemma" xml:id="Gremmel">
          <orth>Gremmel</orth>
          <ptr type="collection" subtype="lemma" target="/collections-wboe.xml#c31407"/>
        </form>
        <gramGrp>
          <gram type="pos">Substantiv</gram>
          <gram type="gender">Femininum</gram>
          <gram type="gender">Neutrum</gram>
        </gramGrp>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.3">öNTir.</placeName>
        </usg>
