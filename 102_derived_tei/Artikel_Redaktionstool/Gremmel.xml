              <usg type="geo">
                <placeName type="kleinregion" ref="/geodata-wboe.xml#p5.1b">nördl.obInnv.</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c31452"/>
          </sense>
          <sense>
            <ref type="form">(Plural)</ref>
            <def>Ausschlag (um den Mund)</def>
