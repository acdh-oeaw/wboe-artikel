            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">kremön vom špög ausbrå</quote>
              <usg type="geo">
                <placeName type="kleinregion" ref="/geodata-wboe.xml#p5.1b">nördl.obInnv.</placeName>
              </usg>
            </cit>
            <cit type="example">
