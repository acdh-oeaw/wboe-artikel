                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p2.1">ObKtn.</placeName>
                </usg>
                <cit type="example">
                  <quote xml:lang="bar-AT">Miar gutzlt die Hånt</quote>
                  <usg type="geo">
                    <placeName type="gemeinde" ref="/geodata-wboe.xml#p1A.1a02">Mals</placeName>
                  </usg>
                </cit>
                <cit type="example">
                  <quote xml:lang="bar-AT">dȫn Viec̄h guzzlts</quote>
                  <usg type="geo">
                    <placeName type="gemeinde" ref="/geodata-wboe.xml#p1A.1a02">Mals</placeName>
                  </usg>
                </cit>
                <cit type="example">
                  <quote xml:lang="bar-AT">ę̆s wę̄rt ihr håll gutzlt hawm</quote>
                  <usg type="geo">
                    <placeName type="gemeinde" ref="/geodata-wboe.xml#p1A.1a02">Mals</placeName>
                  </usg>
                </cit>
                <cit type="example">
                  <quote xml:lang="bar-AT">əs gutzelt mi in dr gånzn kchripp</quote>
                  <usg type="geo">
                    <placeName type="kleinregion" ref="/geodata-wboe.xml#p1A.1h">Umg.Meran</placeName>
                  </usg>
                </cit>
                <cit type="example">
                  <quote xml:lang="bar-AT">Di Haut guzzlt</quote>
                  <usg type="geo">
                    <placeName type="kleinregion" ref="/geodata-wboe.xml#p1A.1i">Pass.</placeName>
                  </usg>
                </cit>
                <cit type="example">
                  <quote xml:lang="bar-AT">S' guz'lt mi in der Nos'n</quote>
                  <usg type="geo">
                    <placeName type="gemeinde" ref="/geodata-wboe.xml#p1C.1m02">Fendels</placeName>
                  </usg>
                </cit>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c44531"/>
              </sense>
            </sense>
          </sense>
          <sense>
            <sense>
              <sense>
