        <sense>
          <sense>
            <sense>
              <sense>
                <def>kitzeln; jemanden durch Berühren an empfindlichen Stellen zum Lachen bringen</def>
                <usg type="geo">
                  <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
                </usg>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p1B.0">OTir.</placeName>
                </usg>
                <usg type="geo">
                  <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
                </usg>
                <usg type="geo">
                  <placeName type="bundesland" ref="/geodata-wboe.xml#p2">Ktn.</placeName>
                </usg>
                <cit type="example">
                  <quote xml:lang="bar-AT">er tuᵃt geᵃrn Menscher
                    <cit type="translation">
                      <quote xml:lang="de">Mädchen</quote>
                    </cit>guz'ln, wenn er a Glasl z'viel hoᵃt
                  </quote>
                  <usg type="geo">
                    <placeName type="gemeinde" ref="/geodata-wboe.xml#p1C.1m02">Fendels</placeName>
                  </usg>
                </cit>
                <cit type="example">
                  <quote xml:lang="bar-AT">ɒ̊n di Fuassōla kån ma r åan ztåad
                    <cit type="translation">
                      <quote xml:lang="de">zu Tode</quote>
                    </cit>guzla
                  </quote>
                  <usg type="geo">
                    <placeName type="gemeinde" ref="/geodata-wboe.xml#p1C.1m06">Nauders</placeName>
                  </usg>
                </cit>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c44532"/>
              </sense>
            </sense>
            <sense>
              <sense>
                <def>jucken; kribbeln; etwas ruft einen unangenehmen Juckreiz hervor</def>
                <usg type="geo">
                  <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
                </usg>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p1B.0">OTir.</placeName>
