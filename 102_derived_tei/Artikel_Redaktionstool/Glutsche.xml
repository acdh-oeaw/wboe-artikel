    <body>
      <entry>
        <form type="lemma" xml:id="Glutsche">
          <orth>Glutsche</orth>
        </form>
        <form type="lemma" xml:id="Glutsch">
          <orth>Glutsch</orth>
          <ptr type="collection" subtype="lemma" target="/collections-wboe.xml#c36791"/>
        </form>
        <gramGrp>
          <gram type="pos">Substantiv</gram>
          <gram type="gender">Femininum</gram>
          <gram type="gender">Maskulinum</gram>
        </gramGrp>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p1B">OTir.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p2">Ktn.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p3.3">WStmk.</placeName>
        </usg>
        <note type="kommentar"></note>
        <etym>
          <seg type="desc">Das Lexem ist vermutlich unter romanischem Einfluss (</seg>
          <cit type="etymon">
            <lang>lad.</lang>
            <form>
              <orth>clocia</orth>
            </form>
          </cit>
          <cit type="etymon">
            <lang>rätorom.</lang>
            <form>
              <orth>clutscha</orth>
            </form>
          </cit>
          <cit type="etymon">
            <lang>ital.</lang>
            <form>
              <orth>chioccia</orth>
            </form>
            <def>Bruthenne</def>
          </cit>
          <seg type="desc">) ins Deutsche gelangt.
            <ref type="bibl" subtype="online">
              <lbl>vgl.</lbl>Sabatini/Coletti online
              <xr type="lemma">
                <ref>chioccia</ref>
                <date type="accessed">06.06.2023</date>
              </xr>
            </ref>
            <ref type="bibl" subtype="online">
              <lbl>vgl.</lbl>DRG online
              <xr type="lemma">
                <ref>clutscha I</ref>
                <date type="accessed">05.06.2023</date>
              </xr>
            </ref>
          </seg>
          <seg type="desc">; siehe auch</seg>
          <cit type="etymon">
            <xr type="lemma">
              <ref target="Glucke.xml#Glucke">Glucke</ref>
            </xr>
          </cit>
        </etym>
        <sense>
          <sense>
            <def>Bruthenne; Gluckhenne</def>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p1B">OTir.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c36792"/>
          </sense>
          <sense>
            <sense>
              <def>schlechtes Bier; schlechter Wein</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.3">WStmk.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c36793"/>
            </sense>
            <sense>
              <def>Fehlstelle beim Mähen mit der Sense, die aufgrund von unsauberem Dengeln und daraus resultierender stumpfer Schneide zustande kommt</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c36795"/>
            </sense>
            <sense>
              <lbl>nur</lbl>
              <xr type="lemma">
                <ref target="#Glutsch">Glutsch</ref>
              </xr>
              <usg>(Maskulinum)</usg>
              <def>schlüpfriger Boden; Glatteis</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p2">Ktn.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c36796"/>
            </sense>
          </sense>
          <sense>
            <def>Herbstzeitlose; Colchicum autumnale L.</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p1A.1">wSTir.</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c36794"/>
          </sense>
        </sense>
