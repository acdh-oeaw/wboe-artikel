            </usg>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p4">Sbg.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p5.3">Traunv.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p5.4">Mühlv.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p6">NÖ</placeName>
            </usg>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p7.1">NBgl.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p7.3">SBgl.</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c42641"/>
          </sense>
          <sense>
            <def>Milde; Erlass oder Herabsetzung einer Strafe</def>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p3">Stmk.</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">Wan i den dawischat, / Der kriagat koa Gnad', / I schlagat den krump'n / Schmarotza frei g'rad</quote>
              <ref type="bibl" target="#Werle1884">Werle 1884
                <citedRange>259</citedRange>
              </ref>
              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p3.2c05">Vordernberg</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c42647"/>
          </sense>
        </sense>
        <re type="redewendung">
