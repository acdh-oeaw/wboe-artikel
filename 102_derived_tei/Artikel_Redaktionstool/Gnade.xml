        </re>
        <re type="redewendung">
          <form subtype="MWE" type="lemma">
            <orth>in der Gnade sein</orth>
          </form>
          <sense>
            <def>bei Gott sein; im Himmel sein</def>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">in der Gnad sein</quote>
              <ref type="bibl" target="#Bredenbrücker1901">STirId. 1901
                <citedRange>728</citedRange>
              </ref>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c42648"/>
          </sense>
        </re>
        <re type="redewendung">
          <form subtype="MWE" type="lemma">
            <orth>sich eine Gnade daraus machen</orth>
          </form>
          <sense>
            <def>sich eine Ehre daraus machen; sich zur Ehre anrechnen</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">Z' Wien muß sich der Gast ein Gnad draus machen</quote>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c42649"/>
          </sense>
          <sense>
            <def>so tun, als ob man eine Wohltat erweisen würde</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">Da muaß ma si' no a Gnad draus mach'n</quote>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c42643"/>
          </sense>
          <sense>
            <def>geschmeichelt sein</def>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
            </usg>
