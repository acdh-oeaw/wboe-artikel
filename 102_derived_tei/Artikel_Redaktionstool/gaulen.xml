    <body>
      <entry>
        <form type="lemma" xml:id="gaulen">
          <orth>gaulen</orth>
          <ptr type="collection" subtype="lemma" target="/collections-wboe.xml#c26098"/>
        </form>
        <gramGrp>
          <gram type="pos">Verb</gram>
          <iType>schwach</iType>
        </gramGrp>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p1A.4">öSTir.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p3">Stmk.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p4.3">Pong.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p7.3">SBgl.</placeName>
        </usg>
        <note type="kommentar" resp="#DG">Der auf mhd.
          <c>ū</c>zurückgehende Stammvokal wurde überall zu
          <c>au</c>diphthongiert
          <xr>
            <ref>gaul̥n</ref>
          </xr>und der Liquid nur im westlichen Südmittelbairischen vokalisiert
          <xr>
            <ref>gau-ẹn</ref>
          </xr>. Der Vokal der Nebensilbe wurde synkopiert
          <xr>
            <ref>gauln</ref>
          </xr>, und im östl. Südtirol der auslautende Nasal zudem assimiliert und
          <c>l</c>silbisch
          <xr>
            <ref>gaul</ref>
          </xr>.
        </note>
        <etym>
          <cit type="etymon">
            <lang>schweiz.</lang>
            <form>
              <orth>gaulen</orth>
            </form>
            <def>possenhaft tändeln</def>
          </cit>
          <ref type="bibl" subtype="online">
            <lbl>vgl.</lbl>DWB
            <xr type="lemma">
              <ref>gaulen</ref>
              <date type="accessed">15.06.2022</date>
            </xr>
          </ref>
          <seg type="desc">; Ableitung von</seg>
          <cit type="etymon">
            <xr type="lemma">
              <ref target="Gaul.xml#Gaul">Gaul</ref>
            </xr>
          </cit>
        </etym>
        <sense>
          <sense>
            <sense>
              <def>ausgelassen (und lärmend) herumtollen, herumspringen, spielen, raufen; sich herumtreiben; gaukeln; lärmen; schreien</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p3">Stmk.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p7.3">SBgl.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c27152"/>
            </sense>
            <sense>
              <def>galoppieren</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.2">mbair.ObStmk.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c27154"/>
            </sense>
          </sense>
          <sense>
            <def>fest arbeiten</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p4.3">Pong.</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c27153"/>
          </sense>
        </sense>
        <re>
          <form subtype="compound" type="lemma">
            <orth>aufgaulen</orth>
          </form>
          <sense>
            <sense>
              <sense>
                <def>den großen Eisenrechen (= Gaul) über Kornäcker ziehen</def>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p1A.4">öSTir.</placeName>
                </usg>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c27155"/>
              </sense>
              <sense>
                <def>Gras auf die Schwedenreiter hängen</def>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p1A.4">öSTir.</placeName>
                </usg>
                <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c27159"/>
              </sense>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>umeinandergaulen</orth>
          </form>
          <sense>
            <sense>
              <def>herumtollen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c27158"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>umgaulen</orth>
          </form>
          <sense>
            <sense>
              <def>herumtollen; herumspringen; herumstreunen; Unfug treiben; spielen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.1">sbair.ObStmk.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.3">WStmk.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.4">MStmk.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.5">OStmk.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p4.3">Pong.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p7.3">SBgl.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">die Buabn gauln den gånzn Tåg um</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p3.3c08">Stainz</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c27156"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>umhergaulen</orth>
          </form>
          <sense>
            <sense>
              <def>herumtollen; herumspringen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.2">mbair.ObStmk.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c27157"/>
            </sense>
          </sense>
        </re>
