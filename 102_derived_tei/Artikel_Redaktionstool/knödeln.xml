        <note type="kommentar"></note>
        <etym>
          <seg type="desc">Ableitung von</seg>
          <cit type="etymon">
            <xr type="lemma">
              <ref target="Knödel.xml#Knoedel">Knödel</ref>
            </xr>
          </cit>
        </etym>
        <sense>
          <sense>
            <def>schwer aus der Kehle bringen; nicht frei bzw. undeutlich sprechen oder singen (als ob man einen Knödel im Hals oder einen Bissen im Mund hätte)</def>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p4">Sbg.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c36992"/>
          </sense>
          <sense>
            <def>mit geballten Fäusten schlagen, stoßen</def>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p2">Ktn.</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c36994"/>
          </sense>
          <sense>
            <def>ärgern</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p6.4">Weinv.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">dås knödelt mi</quote>
              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.1l02">Hofstetten-Grünau</placeName>
              </usg>
            </cit>
            <cit type="example">
              <quote xml:lang="bar-AT">Der Kauf wird'n Knödeln</quote>
              <ref type="bibl" target="#Hügel1873">Hügel 1873
                <citedRange>92</citedRange>
              </ref>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c36995"/>
          </sense>
        </sense>
        <re>
          <form subtype="compound" type="lemma">
            <orth>abknödeln</orth>
          </form>
          <sense>
            <sense>
              <def>mit geballten Fäusten schlagen; jemanden tüchtig verhauen</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p2">Ktn.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.3">WStmk.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c36993"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>einknödeln</orth>
          </form>
          <sense>
            <sense>
              <def>mit geballten Fäusten schlagen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p7.3">SBgl.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c36997"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>hervorknödeln</orth>
          </form>
          <sense>
            <sense>
              <def>nicht frei heraus sprechen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c36998"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>umherknödeln</orth>
          </form>
          <sense>
            <sense>
              <def>umherknüllen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.2">Hausrv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c36999"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>verknödeln</orth>
          </form>
          <sense>
            <sense>
              <def>verknüllen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.2">Hausrv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c37000"/>
            </sense>
            <sense>
              <def type="umschreibung">Ausdruck für eine Leibesstrafe im Kinderspiel</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c37001"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>zusammenknödeln</orth>
          </form>
          <sense>
            <sense>
              <def>zerknüllen; zusammenballen; zusammenknäueln; zu einem Knödel zusammendrücken; zu Kugeln zusammendrehen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.2">mbair.ObStmk.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.2">Hausrv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.2">Industriev.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">s Brā̊d zam̄gnėᵈln</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.1b02">Oed-Oehling</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c37002"/>
            </sense>
          </sense>
        </re>
        <re type="redewendung">
          <form subtype="MWE" type="lemma">
            <orth>jemand hat es sich schon knödeln lassen</orth>
          </form>
          <sense>
            <def>jemand hat es sich schon zurechtgelegt</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">so hab ich mirs schon knödeln lassen, wie's ausgeht</quote>
              <ref type="bibl" target="#Holzer1943">Holzer 1943
                <citedRange>131</citedRange>
              </ref>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c36996"/>
          </sense>
        </re>
