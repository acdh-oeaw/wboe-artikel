    <body>
      <entry>
        <form type="lemma" xml:id="Fuehrerei">
          <orth>Führerei</orth>
          <ptr type="collection" subtype="lemma" target="/collections-wboe.xml#c23016"/>
        </form>
        <gramGrp>
          <gram type="pos">Substantiv</gram>
          <gram type="gender">Femininum</gram>
        </gramGrp>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.1">wNTir.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.3">öNTir.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p6.2">Industriev.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p6.3">Waldv.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
        </usg>
        <note type="kommentar" resp="#PZ">Der auf mhd.
          <c>üe</c>züruckgehende Diphthong der Stammsilbe wurde im gesamten Verbreitungsgebiet entrundet
          <xr>
            <ref>fīərəraį</ref>
            <ref>fīɒrɒraį</ref>
          </xr>. Der Vokal der mittleren Silbe tritt überal reduziert auf. Der auslautende Diphthong wurde in Wien monophthongiert
          <xr>
            <ref>fīɒrɒræ</ref>
          </xr>.
        </note>
        <form type="dialect">
          <pron xml:lang="bar-AT">fīrərai</pron>
          <usg type="geo">
            <placeName type="gemeinde" ref="/geodata-wboe.xml#p1C.1f02">Biberwier</placeName>
          </usg>
        </form>
        <form type="dialect">
          <pron xml:lang="bar-AT">fiₐrarei</pron>
          <usg type="geo">
            <placeName type="gemeinde" ref="/geodata-wboe.xml#p1C.3f02">Hopfgarten im Brixental</placeName>
          </usg>
        </form>
        <form type="dialect">
          <pron xml:lang="bar-AT">-firₐrei</pron>
        </form>
        <form type="dialect">
          <pron xml:lang="bar-AT">-fiᵣₐrei</pron>
        </form>
        <form type="dialect">
          <pron xml:lang="bar-AT">fiᵣₐrei</pron>
          <usg type="geo">
            <placeName type="kleinregion" ref="/geodata-wboe.xml#p2.2d">obstGurkt.</placeName>
          </usg>
        </form>
        <form type="dialect">
          <pron xml:lang="bar-AT">führarei</pron>
          <usg type="geo">
            <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.2h02">Aspang-Markt</placeName>
          </usg>
        </form>
        <form type="dialect">
          <pron xml:lang="bar-AT">fīararāi</pron>
        </form>
        <form type="dialect">
          <pron xml:lang="bar-AT">-fiararei</pron>
          <usg type="geo">
            <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.3a15">Kautzen</placeName>
          </usg>
        </form>
        <form type="dialect">
          <pron xml:lang="bar-AT">fiərərẹⁱ</pron>
          <usg type="geo">
            <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.5r01">Gersthof</placeName>
          </usg>
          <ptr type="collection" subtype="lautung" target="/collections-wboe.xml#c24224"/>
        </form>
        <etym>
          <seg type="desc">Ableitung von</seg>
          <cit type="etymon">
            <xr type="lemma">
              <ref target="Führer.xml#Fuehrer">Führer</ref>
            </xr>
          </cit>
        </etym>
        <sense>
          <sense>
            <sense>
              <def>Vorgang der Leitung (z. B. einer Bergtour)</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.1">wNTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.3">öNTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.2">Industriev.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">Mit der Füᵃhrerei geᵃhts oᵃlli Johr lötzer, 's sein lauter Alloᵃnlafer</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p1C.1m02">Fendels</placeName>
                </usg>
              </cit>
              <cit type="example">
                <quote xml:lang="bar-AT">wăs is dăs fir a Fiₐrarei</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p1C.3f02">Hopfgarten im Brixental</placeName>
                </usg>
              </cit>
              <cit type="example">
                <quote xml:lang="bar-AT">dö ewigi Führarei is ma schoⁿ z' dumm</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.2h02">Aspang-Markt</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c24225"/>
            </sense>
            <sense>
              <def>schlechte/ungeschickte Leitung</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">wås is den des fiər a Fiərərẹⁱ?</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.5r01">Gersthof</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c24226"/>
            </sense>
          </sense>
        </sense>
        <re>
          <form subtype="compound" type="lemma">
            <orth>Eisführerei</orth>
          </form>
          <sense>
            <sense>
              <def type="umschreibung">Ausdruck des Unmutes über die Art der Durchführung der Winterarbeit</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c24227"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>Kriegführerei</orth>
          </form>
          <sense>
            <sense>
              <def type="umschreibung">Ausdruck des Unmutes über die Art und Dauer der Kriegsführung</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c24228"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>Maschinführerei</orth>
          </form>
          <sense>
            <sense>
              <def>Verwendung von Maschinen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.3">Waldv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c24229"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>Prozessführerei</orth>
          </form>
          <sense>
            <sense>
              <def type="umschreibung">Ausdruck des Unmutes über das häufige und unnötige Führen von Gerichtsprozessen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c24230"/>
            </sense>
          </sense>
        </re>
      </entry>
