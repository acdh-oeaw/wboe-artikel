            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>umherkrägeln</orth>
          </form>
          <sense>
            <sense>
              <def>„herumliebeln“</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.2">mbair.ObStmk.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c39029"/>
            </sense>
          </sense>
        </re>
        <re type="redewendung">
          <xr type="variant" subtype="compound">
            <ref>abkrägeln</ref>
          </xr>
          <form subtype="MWE" type="lemma">
            <orth>Der wird bald abkrägeln</orth>
          </form>
          <sense>
            <def>mit dem wird es bald ein Ende nehmen</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p6.4">Weinv.</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">dea wiad bǫl ǫ́̄gragln!</quote>
              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.4d18">Poysdorf</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c39033"/>
          </sense>
        </re>
        <re type="redewendung">
          <form subtype="MWE" type="lemma">
            <orth>Ich werde dich einmal abkrägeln</orth>
          </form>
          <sense>
            <def type="umschreibung">Androhung von Schlägen</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">I węᵃr dį amǫl åbkragln</quote>
              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p2.3g02">Glanegg</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c39031"/>
