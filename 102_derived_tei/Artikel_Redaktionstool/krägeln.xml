        <sense>
          <sense>
            <sense>
              <def>am Hals/Kragen packen; würgen; töten</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1B.0">OTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p2">Ktn.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.2">Hausrv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c39026"/>
            </sense>
            <sense>
              <def>jemanden beim Gratulieren zum Namenstag am Hals packen</def>
              <usg>(scherzhaft)</usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1B.0">OTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.3">öNTir.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">iaz weri di fǫǫ̈t kragln; wei deiⁿ Namenstog is!</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p1C.3e06">Kufstein</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c39036"/>
            </sense>
            <sense>
              <def type="umschreibung">Brauch zur Brechelzeit, bei dem die Brechlerinnen Vorbeikommenden ein Flachsbüschel / eine Hanfsträhne an den Hals binden bzw. den Vorbeikommenden über das Brechelbrett ziehen</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1B.0">OTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p2">Ktn.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p3">Stmk.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">gekchraglt werden</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p2.1f01">Lesachtal</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c39034"/>
            </sense>
          </sense>
          <sense>
            <sense>
              <def>räuspern</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c39044"/>
            </sense>
            <sense>
              <def>schleimrasseln</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.4">Mühlv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c39045"/>
            </sense>
          </sense>
          <sense>
            <def>den Blasiussegen erteilen</def>
            <def type="umschreibung">(Segen am 3. Februar gegen Halsleiden)</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p4.2">Pinzg.</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c39043"/>
          </sense>
          <sense>
            <sense>
              <def>eine Flasche am Hals nehmen und direkt aus der Flasche trinken</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c39037"/>
            </sense>
            <sense>
              <def>den Flaschenhals abschlagen</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1A.4">öSTir.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c39039"/>
            </sense>
            <sense>
              <def>ein Gefäß beim Beerensammeln bis zum Hals füllen</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p5">OÖ</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c39035"/>
            </sense>
          </sense>
          <sense>
            <def>einen langen Hals machen, um irgendwo hineinzuschauen oder gerade hinaufzuschauen</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c39041"/>
          </sense>
          <sense>
            <def>einen Gegenstand zusammendrücken, zusammenbinden</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.3">öNTir.</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c39040"/>
          </sense>
          <sense>
            <def>einen Baum im Winter rundherum einhacken</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p4.2">Pinzg.</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c39042"/>
          </sense>
          <sense>
            <def>Rundholz mit Geschick in die richtige Lage bringen</def>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c39038"/>
          </sense>
        </sense>
        <re>
          <form subtype="compound" type="lemma">
            <orth>abkrägeln</orth>
          </form>
          <sense>
            <sense>
              <def>den Hals umdrehen; würgen; abwürgen; erwürgen; die Kehle durchschneiden; den Kopf abhacken; enthaupten; abstechen; schlachten; töten</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1B.0">OTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p2">Ktn.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p3">Stmk.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p4">Sbg.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p5">OÖ</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p6">NÖ</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p7.1">NBgl.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p7.2">MBgl.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">a Henn' ǭkragln</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p1C.3e06">Kufstein</placeName>
                </usg>
              </cit>
              <cit type="example">
                <quote xml:lang="bar-AT">gragö̱ˡ'n nöd å</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.3b02">Gmunden</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c39027"/>
            </sense>
            <sense>
              <def>(durch Wucher) wirtschaftlich zugrunderichten</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.3">öNTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">da Wuechara had'n ăkhrāgelt</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p1C.3f02">Hopfgarten im Brixental</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c39030"/>
            </sense>
            <sense>
              <def>jemandem (beim Gratulieren) um den Hals fallen; jemandem schön tun</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.3">öNTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.2">mbair.ObStmk.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c39032"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>ankrägeln</orth>
          </form>
          <sense>
            <sense>
              <def>rund herum anschneiden (vom Fell geschlagener Tiere, um es bequem abziehen zu können)</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p3">Stmk.</placeName>
              </usg>
