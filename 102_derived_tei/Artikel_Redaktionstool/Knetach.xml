        <sense>
          <sense>
            <sense>
              <def>Tätigkeit des Knetens; Kneterei (auch abwertend)</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p2">Ktn.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.3">Traunv.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.2">Industriev.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.3">Waldv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c42477"/>
            </sense>
          </sense>
          <sense>
            <sense>
              <def>durcheinander gemischte feuchte Menge; Mischmasch (auch abwertend, z.B. für schlechte Mehlkost)</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.4">Mühlv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c42479"/>
            </sense>
            <sense>
              <def>durchgekneteter Teig (auch abwertend)</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.2">mNTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.3">öNTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c42478"/>
            </sense>
          </sense>
        </sense>
        <re type="redewendung">
          <form subtype="MWE" type="lemma">
            <orth>ein Knetach haben</orth>
          </form>
          <sense>
            <def>sich unruhig bewegen; stürmisch sein</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
            </usg>
          </sense>
          <cit type="example">
            <quote xml:lang="bar-AT">Wås håst den fᵃr a Gᵃknötᵃch?
              <cit type="translation">
                <quote xml:lang="de">rhetorische Frage einer Mutter an ihr sich im Bett hin- und herwälzendes Kind oder einer Frau an ihren Liebhaber, wenn er mit den Liebkosungen zu stürmisch wird</quote>
              </cit>
            </quote>
            <usg type="geo">
              <placeName type="gemeinde" ref="/geodata-wboe.xml#p2.3b05">Straßburg</placeName>
            </usg>
          </cit>
          <ptr type="collection" subtype="MWE" target="/collections-wboe.xml#c42481"/>
        </re>
