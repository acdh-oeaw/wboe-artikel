        <sense>
          <sense>
            <def type="umschreibung">Bezeichnung für Schulkinder, die eine bestimmte (erste, zweite, dritte, vierte) Schulstufe besuchen</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p1A.1">wSTir.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p3.4">MStmk.</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">erstn kchlassler gāgelen / zwǫatn kchlassler schnābelen / drittn kchlassler engelen / fiərtn kchlassler bengelen / finpftn kchlassler hålbr tuifl / sëkstn kchlassler gånze tuifl</quote>
              <usg type="geo">
                <placeName type="kleinregion" ref="/geodata-wboe.xml#p1A.1h">Umg.Meran</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c38489"/>
          </sense>
        </sense>
