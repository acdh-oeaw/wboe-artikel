            </usg>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p4.5">Flachg.</placeName>
            </usg>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p5">OÖ</placeName>
            </usg>
            <usg type="geo">
              <placeName type="bundesland" ref="/geodata-wboe.xml#p6">NÖ</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c42440"/>
          </sense>
        </sense>
