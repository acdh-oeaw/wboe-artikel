    <body>
      <entry>
        <form type="lemma" xml:id="Kleispe">
          <orth>Kleispe</orth>
        </form>
        <form type="lemma" xml:id="Kleispen">
          <orth>Kleispen</orth>
          <ptr type="collection" subtype="lemma" target="/collections-wboe.xml#c41766"/>
        </form>
        <gramGrp>
          <gram type="pos">Substantiv</gram>
          <gram type="gender">Maskulinum</gram>
          <gram type="gender">Femininum</gram>
        </gramGrp>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p1B">OTir.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
        </usg>
        <usg type="geo">
          <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
        </usg>
        <etym>
          <seg type="desc">
            <lang>frnhd.</lang>
          </seg>
          <cit type="etymon">
            <form>
              <orth>kleispe(r)</orth>
            </form>
            <gramGrp>
              <gram type="gender">Maskulinum</gram>
            </gramGrp>
            <def>Splitter</def>
          </cit>
          <ref type="bibl" subtype="online">
            <lbl>vgl.</lbl>Frühnhd. Wb. online
            <xr type="lemma">
              <ref>kleispe(r)</ref>
              <date type="accessed">24.11.2023</date>
            </xr>
          </ref>
          <seg type="desc">, in Tirol ist auch eine Nebenform mit
            <c>-t-</c>anstelle von
            <c>-p-</c>belegt; für die Entwicklung der Bedeutungen ausschlaggebend war offenbar die Ähnlichkeit mit einem Splitter bzw. das Sich-Aufsplittern (vergleiche die Attribute 'faserig' und 'ausgefasert' in den Bedeutungsangaben)
            <ref type="bibl" subtype="online">
              <lbl>vgl.</lbl>DWB online
              <xr type="lemma">
                <ref>kleispe</ref>
                <date type="accessed">24.11.2023</date>
              </xr>
            </ref>; beachte auch im
            <lang>Schweizerdeutschen</lang>
          </seg>
          <cit type="etymon">
            <form>
              <orth>Chlīspeⁿ</orth>
            </form>
            <gramGrp>
              <gram type="gender">Femininum</gram>
            </gramGrp>
          </cit>
          <seg type="desc">mit u. a. der Bedeutung</seg>
          <cit type="etymon">
            <def>Abschnitt, Rest von einem Stück Schuhdraht</def>
          </cit>
          <ref type="bibl" target="#SchweizerischesId1895III">
            <lbl>vgl.</lbl>Schweizerisches Id. 1895 III
            <citedRange>700</citedRange>
          </ref>
        </etym>
        <sense>
          <sense>
            <sense>
              <def>Schweinsborste (zur Verwendung im Schuhmacherhandwerk)</def>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1A">STir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1B">OTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c41808"/>
            </sense>
            <sense>
              <def>einzelnes Stück / Rest eines Schusterdrahtes (eines zum Nähen verwendeten, mit Pech getränkten Hanfgarns)</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1C.1">wNTir.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c41809"/>
            </sense>
            <sense>
              <def>ausgefasertes, faseriges Ende bzw. Spitze des Schusterdrahtes (Schusterzwirns), in welches bzw. welche eine Schweinsborste als Nadel hineingedreht wird</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p1A.4">öSTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1B">OTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p1C">NTir.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c41810"/>
            </sense>
          </sense>
          <sense>
            <def>Hand</def>
            <usg>(gaunersprachlich)</usg>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c41807"/>
          </sense>
        </sense>
        <re type="redewendung">
          <form subtype="MWE" type="lemma">
            <orth>die Kleispe ausziehen</orth>
          </form>
          <sense>
            <def>die Fasern des gedrehten Schuhgarnes zu einer haarfein verlaufenden Spitze ausziehen</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p6.1">Mostv.</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">d'Kleischp'n ausziāŋ</quote>
              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.1q07">Stössing</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c41922"/>
          </sense>
        </re>
        <re type="redewendung">
          <form subtype="MWE" type="lemma">
            <orth>Kleispen drehen</orth>
          </form>
          <sense>
            <def>das Fadenende vom Schusterzwirn zuspitzen</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">Khlaišpm dʀā̃n</quote>
              <usg type="geo">
                <placeName type="gemeinde" ref="/geodata-wboe.xml#p2.2i07">Villach</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c41923"/>
          </sense>
        </re>
      </entry>
