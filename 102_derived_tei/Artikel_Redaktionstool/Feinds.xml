                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p4.2a06">Niedernsill</placeName>
                </usg>
              </cit>
              <cit type="example">
                <quote xml:lang="bar-AT">Håt dö an Feinz</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p4.3c02">Bad Gastein</placeName>
                </usg>
              </cit>
              <cit type="example">
                <quote xml:lang="bar-AT">ãn φẽĩτʃ hą̃m</quote>
                <usg type="geo">
                  <placeName type="kleinregion" ref="/geodata-wboe.xml#p4.3d">Gr.Arlt.</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c150"/>
            </sense>
            <sense>
              <def>Unwille</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p4.3">Pong.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c151"/>
            </sense>
          </sense>
        </sense>
        <re type="redewendung">
          <form subtype="MWE" type="lemma">
            <orth>Das wäre ein Feinds!</orth>
          </form>
          <sense>
            <def type="umschreibung">Ermahnung, vernünftig zu werden; „unterstreichend, wurde mit verstärkter Stimme und sehr deutlich gesprochen"</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p4.3">Pong.</placeName>
            </usg>
            <cit type="example">
              <quote xml:lang="bar-AT">dës wadɒr ɒ ƒẽĩtʃ!</quote>
              <usg type="geo">
                <placeName type="kleinregion" ref="/geodata-wboe.xml#p4.3d">Gr.Arlt.</placeName>
              </usg>
            </cit>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c152"/>
          </sense>
        </re>
