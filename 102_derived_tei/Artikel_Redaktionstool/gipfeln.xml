                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c39016"/>
            </sense>
          </sense>
        </re>
        <re type="redewendung">
          <form subtype="MWE" type="lemma">
            <orth>Türken abgipfeln</orth>
          </form>
          <sense>
            <def>die verblühten Rispen des Maises wegbrechen, damit er schneller reif wird</def>
