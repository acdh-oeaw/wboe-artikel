                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
              </usg>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40080"/>
            </sense>
          </sense>
          <sense>
            <def>auffallend große Nase</def>
            <usg type="geo">
              <placeName type="grossregion" ref="/geodata-wboe.xml#p1A.4">öSTir.</placeName>
            </usg>
            <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c40081"/>
          </sense>
        </sense>
        <re>
          <form subtype="compound" type="lemma">
