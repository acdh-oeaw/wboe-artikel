        <re>
          <form subtype="compound" type="lemma">
            <orth>aufhinkrempeln</orth>
          </form>
          <sense>
            <sense>
              <def>hinaufstreifen bzw. hinaufstülpen (z. B. von einem Ärmel oder einer Hose)</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.2">MKtn.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p5.4">Mühlv.</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">d ėrmmöl ǫėgrę̃mpön</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p5.4b13">Ulrichsberg</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c34079"/>
            </sense>
          </sense>
        </re>
        <re>
          <form subtype="compound" type="lemma">
            <orth>aufkrempeln</orth>
          </form>
          <sense>
            <sense>
              <def>hinaufstreifen bzw. hinaufstülpen (z. B. von einem Ärmel oder einer Hose)</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p2.1">ObKtn.</placeName>
              </usg>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.3">WStmk.</placeName>
