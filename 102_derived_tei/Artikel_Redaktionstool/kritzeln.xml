                <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">kritzl her dein Nam!</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p4.5d05">St. Gilgen</placeName>
                </usg>
