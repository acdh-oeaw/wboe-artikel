        <sense>
          <sense>
            <sense>
              <def>schreiben; von Zeit zu Zeit etwas niederschreiben</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p4.5">Flachg.</placeName>
              </usg>
              <usg type="geo">
