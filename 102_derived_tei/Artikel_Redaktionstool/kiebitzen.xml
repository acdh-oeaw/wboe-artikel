              </usg>
              <usg type="geo">
                <placeName type="bundesland" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
              </usg>
              <cit type="example">
                <quote xml:lang="bar-AT">dẽs giwazn kõ i nit lein</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.1d10">Neuhofen an der Ybbs</placeName>
                </usg>
              </cit>
              <cit type="example">
                <quote xml:lang="bar-AT">Wǫn dȏ der mit sẽĩn Giwazn aufherad</quote>
                <usg type="geo">
                  <placeName type="gemeinde" ref="/geodata-wboe.xml#p6.1d21">Ybbs an der Donau</placeName>
                </usg>
              </cit>
              <cit type="example">
                <quote xml:lang="bar-AT">I' tua'r añ bißl kiebitz'n</quote>
                <usg type="geo">
                  <placeName type="grossregion" ref="/geodata-wboe.xml#p6.5">Wien</placeName>
                </usg>
              </cit>
              <ptr type="collection" subtype="bedeutung" target="/collections-wboe.xml#c38687"/>
            </sense>
            <sense>
              <def>stichelnd reden; necken; anstacheln</def>
              <usg type="geo">
                <placeName type="grossregion" ref="/geodata-wboe.xml#p3.1">sbair.ObStmk.</placeName>
