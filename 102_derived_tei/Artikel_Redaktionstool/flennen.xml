        <etym>
          <seg type="desc">seit dem
            <date>15. Jh.</date>bezeugt, verwandt ist
            <lang>ahd.</lang>
          </seg>
          <cit type="etymon">
            <form>
              <orth>flannēn</orth>
            </form>
            <def>den Mund verziehen</def>
          </cit>
          <seg type="desc">(
            <date>um 1000</date>) und
            <lang>mhd.</lang>
          </seg>
          <cit type="etymon">
            <form>
              <orth>vlans</orth>
            </form>
            <def>Mund; Maul</def>
          </cit>
          <seg type="desc">; vgl. auch im
            <lang>Norwegischen (dial.)</lang>
          </seg>
          <cit type="etymon">
            <form>
              <orth>flina</orth>
            </form>
          </cit>
          <seg type="desc">und im
            <lang>Schwedischen (dial.)</lang>
          </seg>
          <cit type="etymon">
            <form>
              <orth>flīna</orth>
            </form>
            <def>die Zähne zeigen; grinsen</def>
          </cit>
          <seg type="desc">; vgl. auch
            <lang>mhd.</lang>
          </seg>
          <cit type="etymon">
            <form>
              <orth>flenner</orth>
            </form>
            <def>jemand, der weint, heult</def>
          </cit>
          <seg type="desc">; die weitere Herkunft ist unklar;</seg>
          <cit type="etymon">
            <form>
              <orth>flennen</orth>
            </form>
          </cit>
          <seg type="desc">bedeutet zunächst sowohl</seg>
          <cit type="etymon">
            <def>weinen</def>
          </cit>
          <seg type="desc">als auch</seg>
          <cit type="etymon">
            <def>lachen</def>
          </cit>
          <seg type="desc">, erst im
            <date>18. Jh.</date>setzt sich allgemein die erstgenannte Bedeutung durch
          </seg>
          <ref type="bibl">
            <lbl>vgl.</lbl>Kluge 2011
            <citedRange>302</citedRange>
          </ref>
          <ref type="bibl">Pfeifer 2014
            <citedRange>354</citedRange>
          </ref>
          <seg type="desc">. In der Präfixbildung</seg>
          <cit type="etymon">
            <form>
              <orth>verflennen</orth>
            </form>
          </cit>
          <seg type="desc">(siehe unter Wortbildung) kommt es ausgehend von der etymologischen Grundbedeutung</seg>
          <cit type="etymon">
            <def>den Mund verziehen</def>
          </cit>
          <seg type="desc">zu stufenweisen Bedeutungserweiterungen. Über</seg>
          <cit type="etymon">
            <def>eine verstellte Miene machen</def>
          </cit>
          <seg type="desc">kann sich die Bedeutung</seg>
          <cit type="etymon">
            <def>austauschen; (heimlich) Kleinigkeiten tauschen; nicht ganz offiziell kleinere Tauschgeschäfte machen, Tauschhandel betreiben</def>
          </cit>
          <seg type="desc">und der Bedeutungskreis</seg>
          <cit type="etymon">
            <def>über seine Verhältnisse hinaus leben</def>
          </cit>
          <seg type="desc">entwickeln, aus welchem sich die Bedeutung</seg>
          <cit type="etymon">
            <def>vertun; vergeuden</def>
          </cit>
          <seg type="desc">erklären lässt
            <ref type="bibl" subtype="personal">
              <persName>Auskunft Peter Wiesinger</persName>
              <date type="accessed">26.07.2018</date>
            </ref>
          </seg>
          <seg type="desc">.</seg>
        </etym>
